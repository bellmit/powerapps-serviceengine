<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="true" type="event">
 <exec type="guard">
  <conditions>
   <condition lhsfield="MODULE_NAME" operator="=" rhsvalue="UPL"/>
  </conditions>
  <exec text="Open" type="comment">
   <exec type="guard">
    <conditions>
     <condition lhsfield="WLT_CODE" operator="=" rhsvalue="open"/>
    </conditions>
    <exec event="wltQuery.netOpenAccount" ignoreresult="true" type="raise"/>
   </exec>
  </exec>
  <exec text="Paid" type="comment">
   <exec type="guard">
    <conditions>
     <condition lhsfield="WLT_CODE" operator="=" rhsvalue="paid"/>
    </conditions>
    <exec event="wltQuery.netPaidAccount" ignoreresult="true" type="raise"/>
    <exec from="PAID_ACCOUNT_LIST" to="ACCOUNT_LIST" type="copy"/>
   </exec>
  </exec>
  <exec text="Work In Progress" type="comment">
   <exec type="guard">
    <conditions>
     <condition lhsfield="WLT_CODE" operator="=" rhsvalue="wip"/>
    </conditions>
    <exec event="wltQuery.netWipAccount" ignoreresult="true" type="raise"/>
   </exec>
  </exec>
  <exec text="PTP" type="comment">
   <exec type="guard">
    <conditions>
     <condition lhsfield="WLT_CODE" operator="=" rhsvalue="ptp"/>
    </conditions>
    <exec event="wltQuery.netPtpAccount" ignoreresult="true" type="raise"/>
   </exec>
  </exec>
  <exec text="Broken Promise" type="comment">
   <exec type="guard">
    <conditions>
     <condition lhsfield="WLT_CODE" operator="=" rhsvalue="brokenpromise"/>
    </conditions>
    <exec event="wltQuery.netBrokenPromise" ignoreresult="true" type="raise"/>
   </exec>
  </exec>
  <exec text="Supervisory" type="comment">
   <exec type="guard">
    <conditions>
     <condition lhsfield="WLT_CODE" operator="=" rhsvalue="supervisory"/>
    </conditions>
    <exec argument="ACCOUNT_LIST" query="getSupervisoryAccountData" type="query"/>
   </exec>
  </exec>
  <exec text="Priority " type="comment">
   <exec type="guard">
    <conditions>
     <condition lhsfield="WLT_CODE" operator="=" rhsvalue="priority"/>
    </conditions>
    <exec argument="ACCOUNT_LIST" query="getPriorityAccountData" type="query"/>
   </exec>
  </exec>
  <exec text="R and R" type="comment">
   <exec type="guard">
    <conditions>
     <condition lhsfield="WLT_CODE" operator="=" rhsvalue="randr"/>
    </conditions>
    <exec argument="ACCOUNT_LIST" query="getRandRAccountData" type="query"/>
   </exec>
  </exec>
  <exec text="Agency Assigned" type="comment">
   <exec type="guard">
    <conditions>
     <condition lhsfield="WLT_CODE" operator="=" rhsvalue="agencyassigned"/>
    </conditions>
    <exec argument="ACCOUNT_LIST" query="getAgencyAssignedAccountData" type="query"/>
   </exec>
  </exec>
  <exec text="Unpaid" type="comment">
   <exec type="guard">
    <conditions>
     <condition lhsfield="WLT_CODE" operator="=" rhsvalue="unpaid"/>
    </conditions>
    <exec event="wltQuery.netUnPaidAccount" ignoreresult="true" type="raise"/>
   </exec>
  </exec>
  <exec text="Payment Plan" type="comment">
   <exec type="guard">
    <conditions>
     <condition lhsfield="WLT_CODE" operator="=" rhsvalue="paymentplan"/>
    </conditions>
    <exec argument="ACCOUNT_LIST" query="getPaymentPlanAccountData" type="query"/>
   </exec>
  </exec>
  <exec text="Visitation" type="comment">
   <exec type="guard">
    <conditions>
     <condition lhsfield="WLT_CODE" operator="=" rhsvalue="sitevisitation"/>
    </conditions>
    <exec argument="ACCOUNT_LIST" query="getSiteVisitationAccountData" type="query"/>
   </exec>
  </exec>
  <exec event="wltQuery.getInfo" ignoreresult="true" type="raise"/>
  <exec list="ACCOUNT_LIST" to="COUNT_ACCOUNT_LIST" type="countlist"/>
  <exec type="guard">
   <conditions>
    <condition lhsfield="COUNT_ACCOUNT_LIST" operator="=" rhsvalue="0"/>
   </conditions>
   <exec type="nullify">
    <key value="ACCOUNT_LIST"/>
   </exec>
  </exec>
 </exec>
</event>
