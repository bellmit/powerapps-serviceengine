<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec type="guard">
  <conditions>
   <condition lhsfield="CUSTOMER_ID" operator="is not empty" rhsvalue=""/>
   <condition lhsfield="CUSTOMER_ID" operator="&lt;&gt;" rhsvalue="parent.CUSTOMER_ID"/>
   <condition lhsfield="ACCOUNT_ID" operator="is not empty" rhsvalue=""/>
   <condition lhsfield="ACCOUNT_ID" operator="&lt;&gt;" rhsvalue="parent.ACCOUNT_ID"/>
  </conditions>
  <exec count="1" type="createlist"/>
  <exec type="guard">
   <conditions>
    <condition lhsfield="_HP" operator="=" rhsvalue="SHOW"/>
   </conditions>
   <exec from="SELECTED_ACCOUNT_ID1" key="ACCOUNT_ID" type="copyintolist"/>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="_MG" operator="=" rhsvalue="SHOW"/>
   </conditions>
   <exec from="SELECTED_ACCOUNT_ID2" key="ACCOUNT_ID" type="copyintolist"/>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="_PL" operator="=" rhsvalue="SHOW"/>
   </conditions>
   <exec from="SELECTED_ACCOUNT_ID3" key="ACCOUNT_ID" type="copyintolist"/>
  </exec>
  <exec type="loop">
   <exec argument="CONDUCT_OF_ACCOUNT" query="getConductOfAccount" type="query"/>
   <exec argument="24_MONTH_PROFILE_AGENCY" query="get24MonthsAgencyProfile" type="query"/>
  </exec>
 </exec>
</event>
