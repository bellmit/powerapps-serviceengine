 <exec type="guard">
  <conditions>
   <condition lhsfield="PRODUCT_CATEGORY" operator="=" rhsvalue="PF"/>
  </conditions>
  <exec from="VARIABLE_SETTLEMENT_LIST" to="MG_VARIABLE_SETTLEMENT_LIST" type="copy"/>
 </exec>
 <exec type="guard">
  <conditions>
   <condition lhsfield="PRODUCT_CATEGORY" operator="=" rhsvalue="OD"/>
  </conditions>
  <exec from="VARIABLE_SETTLEMENT_LIST" to="MG_VARIABLE_SETTLEMENT_LIST" type="copy"/>
 </exec>
