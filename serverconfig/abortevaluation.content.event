<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec statement="getAbortEvaluationTreeDocumentId" type="select"/>
 <exec statement="getTreatmentSubtypeForTreeConditions" type="select"/>
 <exec type="guard">
  <conditions>
   <condition lhsfield="SUBTYPE_CODE" operator="=" rhsvalue="ECA"/>
  </conditions>
  <exec statement="getDecisionTreeTypeECA" type="select"/>
 </exec>
 <exec type="guard">
  <conditions>
   <condition lhsfield="SUBTYPE_CODE" operator="&lt;&gt;" rhsvalue="ECA"/>
  </conditions>
  <exec statement="getDecisionTreeTypeLegal" type="select"/>
 </exec>
 <exec type="parallelquery">
  <query value="getDecisionTreeCondition"/>
  <query value="getAbortEvaluationTreeBuckets"/>
  <target value="CONDITIONS"/>
  <target value="BUCKETS"/>
 </exec>
 <exec type="guard">
  <conditions>
   <condition lhsfield="DOCUMENT_ID" operator="is empty" rhsvalue=""/>
  </conditions>
  <exec message="&lt;?xml version=&quot;1.0&quot; encoding=&quot;UTF-8&quot; standalone=&quot;no&quot;?&gt;&lt;node&gt;&lt;/node&gt;" target="CONTENT" type="formattext"/>
 </exec>
 <exec type="guard">
  <conditions>
   <condition lhsfield="DOCUMENT_ID" operator="is not empty" rhsvalue=""/>
  </conditions>
  <exec content="CONTENT" documentid="DOCUMENT_ID" documenttype="9" type="fetchdecisiontree"/>
 </exec>
 <exec count="1" type="createlist"/>
 <exec from="BUCKETS" key="BUCKETS" type="copyintolist"/>
 <exec from="CONDITIONS" key="CONDITIONS" type="copyintolist"/>
 <exec from="CONTENT" key="CONTENT" type="copyintolist"/>
</event>
