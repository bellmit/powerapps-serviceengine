<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec list="SELECTED_DATA" to="COUNT" type="countlist"/>
 <exec type="guard">
  <conditions>
   <condition lhsfield="COUNT" operator="=" rhsvalue="1"/>
  </conditions>
  <exec from="SELECTED_DATA" type="copy"/>
  <exec from="TRUE" key="LOAD_BREAKDOWN" type="copyintolist"/>
 </exec>
 <exec type="guard">
  <conditions>
   <condition lhsfield="COUNT" operator="&lt;&gt;" rhsvalue="1"/>
  </conditions>
  <exec count="1" type="createlist"/>
  <exec from="TRUE" key="CLEAR_BREAKDOWN" type="copyintolist"/>
 </exec>
</event>
