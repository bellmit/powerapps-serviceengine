<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec statement="selectNegativeOne" type="select"/>
 <exec statement="getTreatmentAccountReview" type="select"/>
 <exec type="parallelquery">
  <query value="getYesNoPleaseSelect"/>
  <query value="getAccountReviewStatus"/>
  <query value="getAccountReviewRecoverableStatus"/>
  <target value="YES_NO_LIST"/>
  <target value="REVIEW_STATUS_LIST"/>
  <target value="RECOVERABLE_STATUS_LIST"/>
 </exec>
 <exec type="parallelquery">
  <query value="getAccountReviewDefaultReason"/>
  <query value="getAccountReviewCustomers"/>
  <target value="REASON_OF_DEFAULT_LIST"/>
  <target value="CUSTOMER_LIST"/>
 </exec>
 <exec from="NEGATIVE_ONE" key="NEGATIVE_ONE" to="CUSTOMER_LIST" type="copyintolist"/>
 <exec argument="CUSTOMER_LIST" index="SELECTED" type="loop">
  <exec lhs="SELECTED" rhs="NEGATIVE_ONE" target="SELECTED" type="add"/>
 </exec>
 <exec key="IS_SELECTED" minuend="CUSTOMER_LIST" subtrahend="FALSE" to="SELECTED_LIST" type="remove"/>
 <exec key="SELECTED" list="SELECTED_LIST" to="SELECTED" type="sumlist"/>
</event>
