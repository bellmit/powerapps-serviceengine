<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="true" type="event">
 <exec count="1" type="createlist"/>
 <exec ceiling="false" endmonth="false" nextweekday="false" startmonth="false" to="CURRENT_TIME" truncate="false" type="date"/>
 <exec to="LOGON_USER" type="user"/>
 <exec type="guard">
  <conditions>
   <condition lhsfield="SELECTED_AGENCIES" operator="is not empty" rhsvalue=""/>
  </conditions>
  <exec statement="getContractAgencySubType" type="select"/>
  <exec from="CONTRACT_ID" key="CONTRACT_ID" to="SELECTED_AGENCIES" type="copyintolist"/>
  <exec from="LOGON_USER" key="LOGON_USER" to="SELECTED_AGENCIES" type="copyintolist"/>
  <exec from="CURRENT_TIME" key="CURRENT_TIME" to="SELECTED_AGENCIES" type="copyintolist"/>
  <exec type="guard">
   <conditions>
    <condition lhsfield="SUBTYPE" operator="=" rhsvalue="ECA"/>
   </conditions>
   <exec message="Agency assigned" target="MESSAGE" type="formattext"/>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="SUBTYPE" operator="&lt;&gt;" rhsvalue="ECA"/>
   </conditions>
   <exec message="Legal firm assigned" target="MESSAGE" type="formattext"/>
  </exec>
  <exec argument="SELECTED_AGENCIES" type="loop">
   <exec statement="insertContractAgencyRel" type="insert"/>
   <exec statement="insertContractAgencyRelHistory" type="insert"/>
  </exec>
 </exec>
 <exec from="TRUE" key="CLOSE" type="copyintolist"/>
 <exec from="MESSAGE" key="MESSAGE" type="copyintolist"/>
 <exec from="TRUE" key="OPEN_MESSAGE" type="copyintolist"/>
</event>
