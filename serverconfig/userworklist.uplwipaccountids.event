<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec text="Fetch Paid Accounts" type="comment">
  <exec event="userworklist.uplpaidaccountids" ignoreresult="true" type="raise"/>
 </exec>
 <exec text="Fetch PTP Accounts" type="comment">
  <exec event="userworklist.getptpaccountids" ignoreresult="true" type="raise"/>
 </exec>
 <exec text="Fetch Broken PTP Accounts" type="comment">
  <exec argument="BROKEN_PROMISE_ACCOUNT_LIST" query="getBrokenPtpAccountData" type="query"/>
  <exec from="BROKEN_PROMISE_ACCOUNT_LIST" key="ACCOUNT_ID" to="BROKEN_PROMISE_ACCOUNT_ID_LIST" type="extract"/>
 </exec>
 <exec text="Fetch WIP Accounts" type="comment">
  <exec event="userworklist.wipaccountids" ignoreresult="true" type="raise"/>
 </exec>
 <exec text="Get net WIP accounts" type="comment">
  <exec minuend="IN_PROGRESS_ACCOUNT_LIST" subtrahend="PAID_ACCOUNT_ID_LIST" to="IN_PROGRESS_ACCOUNT_LIST" type="remove"/>
  <exec minuend="IN_PROGRESS_ACCOUNT_LIST" subtrahend="PTP_ACCOUNT_ID_LIST" to="IN_PROGRESS_ACCOUNT_LIST" type="remove"/>
  <exec minuend="IN_PROGRESS_ACCOUNT_LIST" subtrahend="BROKEN_PROMISE_ACCOUNT_ID_LIST" to="IN_PROGRESS_ACCOUNT_LIST" type="remove"/>
  <exec from="IN_PROGRESS_ACCOUNT_LIST" to="WIP_ACCOUNT_ID_LIST" type="copy"/>
 </exec>
 <exec text="Get net SI accounts" type="comment">
  <exec minuend="SI_ACCOUNTS" subtrahend="PAID_ACCOUNT_ID_LIST" to="SI_ACCOUNTS" type="remove"/>
  <exec minuend="SI_ACCOUNTS" subtrahend="PTP_ACCOUNT_ID_LIST" to="SI_ACCOUNTS" type="remove"/>
  <exec minuend="SI_ACCOUNTS" subtrahend="BROKEN_PROMISE_ACCOUNT_ID_LIST" to="SI_ACCOUNTS" type="remove"/>
 </exec>
 <exec text="Get net PDC accounts" type="comment">
  <exec minuend="PDC_ACCOUNTS" subtrahend="PAID_ACCOUNT_ID_LIST" to="PDC_ACCOUNTS" type="remove"/>
  <exec minuend="PDC_ACCOUNTS" subtrahend="PTP_ACCOUNT_ID_LIST" to="PDC_ACCOUNTS" type="remove"/>
  <exec minuend="PDC_ACCOUNTS" subtrahend="BROKEN_PROMISE_ACCOUNT_ID_LIST" to="PDC_ACCOUNTS" type="remove"/>
 </exec>
 <exec text="Get net OCA accounts" type="comment">
  <exec minuend="OCA_ACCOUNTS" subtrahend="PAID_ACCOUNT_ID_LIST" to="OCA_ACCOUNTS" type="remove"/>
  <exec minuend="OCA_ACCOUNTS" subtrahend="PTP_ACCOUNT_ID_LIST" to="OCA_ACCOUNTS" type="remove"/>
  <exec minuend="OCA_ACCOUNTS" subtrahend="BROKEN_PROMISE_ACCOUNT_ID_LIST" to="OCA_ACCOUNTS" type="remove"/>
 </exec>
</event>
