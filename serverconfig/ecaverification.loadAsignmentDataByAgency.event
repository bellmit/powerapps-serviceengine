<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec count="1" type="createlist"/>
 <exec type="parallelquery">
  <query value="getEcaAssignmentVerification"/>
  <query value="getEcaAssignedSummaryAssignmentVerification"/>
  <query value="getEcaNewAssignmentVerification"/>
  <target value="AGENCY_REF"/>
  <target value="ASSIGNED_SUMMARY"/>
  <target value="DATA"/>
 </exec>
 <exec from="ASSIGNED_SUMMARY" key="AGENCY_ID" to="DATA" type="merge"/>
 <exec from="AGENCY_REF" key="AGENCY_ID" to="DATA" type="merge"/>
 <exec from="TRUE" key="enable" to="DATA" type="copyintolist"/>
 <exec argument="DATA" type="loop">
  <exec lhs="ASSIGNED_ACCOUNT" rhs="NEW_ACCOUNT" target="TOTAL_ACCOUNT" type="add"/>
  <exec lhs="ASSIGNED_GROSS" rhs="NEW_GROSS" target="TOTAL_GROSS" type="add"/>
 </exec>
 <exec type="guard">
  <conditions>
   <condition lhsfield="DATA" operator="is not empty" rhsvalue=""/>
  </conditions>
  <exec count="1" to="TOTAL" type="createlist"/>
  <exec message="Total" target="Total" type="formattext"/>
  <exec key="TOTAL_ACCOUNT" list="DATA" to="TOTAL_ACCOUNT" type="sumlist"/>
  <exec key="TOTAL_GROSS" list="DATA" to="TOTAL_GROSS" type="sumlist"/>
  <exec key="ASSIGNED_ACCOUNT" list="DATA" to="ASSIGNED_ACCOUNT" type="sumlist"/>
  <exec key="ASSIGNED_GROSS" list="DATA" to="ASSIGNED_GROSS" type="sumlist"/>
  <exec key="NEW_ACCOUNT" list="DATA" to="NEW_ACCOUNT" type="sumlist"/>
  <exec key="NEW_GROSS" list="DATA" to="NEW_GROSS" type="sumlist"/>
  <exec from="Total" key="AGENCY_NAME" to="TOTAL" type="copyintolist"/>
  <exec from="TOTAL_ACCOUNT" key="TOTAL_ACCOUNT" to="TOTAL" type="copyintolist"/>
  <exec from="TOTAL_GROSS" key="TOTAL_GROSS" to="TOTAL" type="copyintolist"/>
  <exec from="ASSIGNED_ACCOUNT" key="ASSIGNED_ACCOUNT" to="TOTAL" type="copyintolist"/>
  <exec from="ASSIGNED_GROSS" key="ASSIGNED_GROSS" to="TOTAL" type="copyintolist"/>
  <exec from="NEW_ACCOUNT" key="NEW_ACCOUNT" to="TOTAL" type="copyintolist"/>
  <exec from="NEW_GROSS" key="NEW_GROSS" to="TOTAL" type="copyintolist"/>
  <exec from="TOTAL" to="DATA" type="append"/>
 </exec>
 <exec from="DATA" key="DATA" type="copyintolist"/>
 <exec from="TRUE" key="CLEAR_BREAKDOWN" type="copyintolist"/>
 <exec from="SELECTED" key="SELECTED" type="copyintolist"/>
 <exec type="guard">
  <conditions>
   <condition lhsfield="DATA" operator="is empty" rhsvalue=""/>
  </conditions>
  <exec from="TRUE" key="OPEN_MESSAGE" type="copyintolist"/>
  <exec message="No record found" target="MESSAGE" type="formattext"/>
  <exec from="MESSAGE" key="MESSAGE" type="copyintolist"/>
 </exec>
</event>
