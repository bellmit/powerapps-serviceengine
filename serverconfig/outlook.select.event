<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec statement="getTreatmentOutlook" type="select"/>
 <exec statement="getOutlookAccountDetails" type="select"/>
 <exec type="parallelquery">
  <query value="getOutlookPaymentType"/>
  <query value="getOutlookPaymentReason"/>
  <query value="getOutlookContactSource"/>
  <query value="getYesNoPleaseSelect"/>
  <target value="PAYMENT_TYPE_LIST"/>
  <target value="PAYMENT_REASON_LIST"/>
  <target value="CONTACT_SOURCE_LIST"/>
  <target value="BDR_DECLARE_LIST"/>
 </exec>
 <exec type="guard">
  <conditions>
   <condition lhsfield="ACCOUNT_TYPE" operator="=" rhsvalue="NPF"/>
  </conditions>
  <exec argument="CONFIDENCE_LEVEL_LIST" query="getOutlookConfidenceLevelNpf" type="query"/>
 </exec>
 <exec type="guard">
  <conditions>
   <condition lhsfield="ACCOUNT_TYPE" operator="=" rhsvalue="BDR"/>
  </conditions>
  <exec argument="CONFIDENCE_LEVEL_LIST" query="getOutlookConfidenceLevelBdr" type="query"/>
 </exec>
 <exec type="guard">
  <conditions>
   <condition lhsfield="ACCOUNT_TYPE" operator="=" rhsvalue="NIL"/>
  </conditions>
  <exec statement="getEmptyConfidenceLevel" type="select"/>
  <exec from="IS_NOT_IL_BDR" key="IS_NOT_IL_BDR" to="DEFAULT_LIST" type="copyintolist"/>
 </exec>
 <exec statement="selectOne" type="select"/>
 <exec from="TRUE" key="TRUE" to="PAYMENT_TYPE_LIST" type="copyintolist"/>
 <exec from="FALSE" key="FALSE" to="PAYMENT_TYPE_LIST" type="copyintolist"/>
 <exec from="OUTSTANDING_AMT" key="OUTSTANDING_AMT" to="PAYMENT_TYPE_LIST" type="copyintolist"/>
 <exec from="CURRENT_NLB" key="CURRENT_NLB" to="PAYMENT_TYPE_LIST" type="copyintolist"/>
 <exec from="OPENING_NLB" key="OPENING_NLB" to="PAYMENT_TYPE_LIST" type="copyintolist"/>
 <exec from="SAVED_REVISED_AMOUNT" key="SAVED_REVISED_AMOUNT" to="PAYMENT_TYPE_LIST" type="copyintolist"/>
 <exec from="ZERO" key="ZERO" to="PAYMENT_TYPE_LIST" type="copyintolist"/>
 <exec from="NEGATIVE_ONE" key="NEGATIVE_ONE" to="PAYMENT_TYPE_LIST" type="copyintolist"/>
 <exec argument="PAYMENT_TYPE_LIST" type="loop">
  <exec type="guard">
   <conditions>
    <condition lhsfield="CODE" operator="=" rhsvalue="DCLS"/>
   </conditions>
   <exec from="SAVED_REVISED_AMOUNT" to="REVISED_AMOUNT" type="copy"/>
   <exec from="FALSE" to="enableRevisedAmount" type="copy"/>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="CODE" operator="&lt;&gt;" rhsvalue="DCLS"/>
    <condition lhsfield="CODE" operator="is not empty" rhsvalue=""/>
   </conditions>
   <exec from="ZERO" to="REVISED_AMOUNT" type="copy"/>
   <exec from="TRUE" to="enableRevisedAmount" type="copy"/>
  </exec>
 </exec>
</event>
