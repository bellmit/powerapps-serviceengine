<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec event="wlm.getUserType" ignoreresult="true" type="raise"/>
 <exec from="WLM_WORKLIST_ADD_WORKLIST_NAME" to="WORK_LIST_NAME" type="copy"/>
 <exec from="WLM_WORKLIST_ADD_WORKLIST_DESC" to="DESCRIPTION" type="copy"/>
 <exec ceiling="false" endmonth="false" nextweekday="false" startmonth="false" to="WORK_LIST_CREATE_DATE" truncate="false" type="date"/>
 <exec from="WORK_LIST_CREATE_DATE" to="UPDATE_TIME" type="copy"/>
 <exec to="USER_ID" type="user"/>
 <exec from="WORK_LIST_GROUP_ID" to="WORK_LIST_GROUP" type="copy"/>
 <exec type="transaction">
  <exec type="guard">
   <conditions>
    <condition lhsfield="MODULE_NAME" operator="&lt;&gt;" rhsvalue="RNR"/>
   </conditions>
   <exec type="guard">
    <conditions>
     <condition lhsfield="TYPE_CODE" operator="=" rhsvalue="agencyuser"/>
    </conditions>
    <exec from="EXTERNAL_PROCESS_TYPE_ID" to="WORK_LIST_GROUP_ID" type="copy"/>
    <exec statement="insertAgencyWorkList" type="insert"/>
   </exec>
   <exec type="guard">
    <conditions>
     <condition lhsfield="TYPE_CODE" operator="=" rhsvalue="bankuser"/>
    </conditions>
    <exec statement="insertWlmWorkList" type="insert"/>
   </exec>
   <exec event="wlm.getWlmWorkLists" ignoreresult="true" type="raise"/>
  </exec>
  <exec from="RESULT_LIST" type="copy"/>
 </exec>
</event>
