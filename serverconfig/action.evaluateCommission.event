<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec statement="getActionCategoryForCommission" type="select"/>
 <exec type="guard">
  <conditions>
   <condition lhsfield="DISBURSEMENT" operator="=" rhsvalue="true"/>
  </conditions>
  <exec lhs="DISBURSEMENT_AMOUNT" rhs="SST_PERCENT" target="SST_AMOUNT" type="multiply"/>
  <exec recordcount="" statement="setLegalDisbursement" type="update"/>
 </exec>
 <exec type="guard">
  <conditions>
   <condition lhsfield="DISBURSEMENT" operator="&lt;&gt;" rhsvalue="true"/>
  </conditions>
  <exec argument="TERMS" query="getContractTermForEvaluation" type="query"/>
  <exec from="PROCESS_ID" key="PROCESS_ID" to="TERMS" type="copyintolist"/>
  <exec from="ACTION_ID" key="ACTION_ID" to="TERMS" type="copyintolist"/>
  <exec from="STATUS_ID" key="STATUS_ID" to="TERMS" type="copyintolist"/>
  <exec from="AGENCY_ASSIGNMENT_ID" key="AGENCY_ASSIGNMENT_ID" to="TERMS" type="copyintolist"/>
  <exec from="ACTION_CATEGORY" key="ACTION_CATEGORY" to="TERMS" type="copyintolist"/>
  <exec from="AUCTION_STATUS_ID" key="AUCTION_STATUS_ID" to="TERMS" type="copyintolist"/>
  <exec from="INSTITUTION_LEVEL_ID" key="INSTITUTION_LEVEL_ID" to="TERMS" type="copyintolist"/>
  <exec from="CONTESTED_STATUS_ID" key="CONTESTED" to="TERMS" type="copyintolist"/>
  <exec from="SST_PERCENT" key="SST_PERCENT" to="TERMS" type="copyintolist"/>
  <exec argument="TERMS" type="loop">
   <exec event="action.commission" ignoreresult="true" type="raise"/>
  </exec>
 </exec>
</event>
