<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="true" type="event">
 <exec ceiling="false" endmonth="false" nextweekday="false" startmonth="false" to="CURRENT_TIME" truncate="false" type="date"/>
 <exec to="LOGON_USER" type="user"/>
 <exec from="CURRENT_TIME" key="CURRENT_TIME" to="ACCOUNT_LIST" type="copyintolist"/>
 <exec from="LOGON_USER" key="LOGON_USER" to="ACCOUNT_LIST" type="copyintolist"/>
 <exec from="ABORT_REASON_ID" key="ABORT_REASON_ID" to="ACCOUNT_LIST" type="copyintolist"/>
 <exec from="STATUS_ID" key="STATUS_ID" to="ACCOUNT_LIST" type="copyintolist"/>
 <exec argument="ACCOUNT_LIST" type="loop">
  <exec recordcount="" statement="updateRejectAssignment" type="update"/>
  <exec statement="clearApproval" type="delete"/>
  <exec statement="clearRecommendation" type="delete"/>
  <exec statement="clearMonitoring" type="delete"/>
  <exec statement="abortAssignment" type="insert"/>
  <exec recordcount="UPDATE_COUNT" statement="updatedAccountRejectedAssignment" type="update"/>
  <exec type="guard">
   <conditions>
    <condition lhsfield="UPDATE_COUNT" operator="=" rhsvalue="0"/>
   </conditions>
   <exec statement="insertAccountRejectedAssignment" type="insert"/>
  </exec>
  <exec recordcount="UPDATE_QUEUE" statement="updatedAccountQueueAssignment" type="update"/>
  <exec type="guard">
   <conditions>
    <condition lhsfield="UPDATE_QUEUE" operator="=" rhsvalue="0"/>
   </conditions>
   <exec statement="insertAccountQueueAssignment" type="insert"/>
  </exec>
 </exec>
 <exec count="1" type="createlist"/>
 <exec message="Rejected" target="MESSAGE" type="formattext"/>
 <exec from="TRUE" key="CLOSE" type="copyintolist"/>
 <exec from="TRUE" key="OPEN_MESSAGE" type="copyintolist"/>
 <exec from="MESSAGE" key="MESSAGE" type="copyintolist"/>
</event>
