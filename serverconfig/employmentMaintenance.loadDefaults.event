<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec type="guard">
  <conditions>
   <condition lhsfield="PROCESS_TYPE_ID" operator="is not empty" rhsvalue=""/>
  </conditions>
  <exec count="1" type="createlist"/>
  <exec type="parallelquery">
   <query value="getDefaultTreatmentStatusType"/>
   <query value="getEmploymentMaintenanceEmployerCategory"/>
   <query value="getEmploymentMaintenanceEmployerCode"/>
   <query value="getEmploymentMaintenanceEmploymentStatus"/>
   <target value="TYPE_STATUS_LIST"/>
   <target value="EMPLOYER_CATEGORY_LIST"/>
   <target value="EMPLOYER_LIST"/>
   <target value="EMPLOYMENT_STATUS_LIST"/>
  </exec>
  <exec from="TYPE_STATUS_LIST" key="TYPE_STATUS_LIST" type="copyintolist"/>
  <exec from="EMPLOYER_CATEGORY_LIST" key="EMPLOYER_CATEGORY_LIST" type="copyintolist"/>
  <exec from="EMPLOYER_LIST" key="EMPLOYER_LIST" type="copyintolist"/>
  <exec from="EMPLOYMENT_STATUS_LIST" key="EMPLOYMENT_STATUS_LIST" type="copyintolist"/>
 </exec>
</event>
