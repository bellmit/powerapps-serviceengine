   <exec message="9" target="MISC_CHARGES" type="formattext"/>
   <exec message="10" target="ACCR_COMM_FEE" type="formattext"/>
   <exec message="11" target="OS_BAL" type="formattext"/>
   <exec message="12" target="INTEREST_REBATE" type="formattext"/>
   <exec message="13" target="DEALER_REBATE" type="formattext"/>
   <exec message="14" target="NET_LOSS" type="formattext"/>
