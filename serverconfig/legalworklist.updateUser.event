<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="true" type="event">
 <exec count="1" type="createlist"/>
 <exec type="guard">
  <conditions>
   <condition lhsfield="ID" operator="is not empty" rhsvalue=""/>
  </conditions>
  <exec type="guard">
   <conditions>
    <condition lhsfield="DEFAULT_USER" operator="=" rhsvalue="true"/>
   </conditions>
   <exec recordcount="" statement="clearCurrentUserDefaults" type="update"/>
   <exec recordcount="" statement="clearCurrentUserHistoryDefaults" type="update"/>
  </exec>
  <exec recordcount="" statement="updateAssignedUser" type="update"/>
  <exec recordcount="" statement="updateAssignedUserHistory" type="update"/>
  <exec message="User updated" target="MESSAGE" type="formattext"/>
 </exec>
 <exec from="TRUE" key="CLOSE" type="copyintolist"/>
 <exec from="MESSAGE" key="MESSAGE" type="copyintolist"/>
 <exec from="TRUE" key="OPEN_MESSAGE" type="copyintolist"/>
</event>
