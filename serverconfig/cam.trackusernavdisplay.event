<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="true" type="event">
 <exec from="TRUE" to="VALUE" type="copy"/>
 <exec message="NAVIGATION_DISPLAY" target="VALUE_CODE" type="formattext"/>
 <exec to="LOGON_USER" type="user"/>
 <exec statement="deleteUserSetting" type="delete"/>
 <exec type="guard">
  <conditions>
   <condition lhsfield="_NAVIGATION" operator="=" rhsvalue="HIDE"/>
  </conditions>
  <exec from="FALSE" to="VALUE" type="copy"/>
 </exec>
 <exec statement="insertUserSettings" type="insert"/>
</event>
