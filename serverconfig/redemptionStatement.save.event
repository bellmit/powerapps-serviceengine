<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec type="guard">
  <conditions>
   <condition lhsfield="IS_NEW_TREATMENT" operator="=" rhsvalue="true"/>
  </conditions>
  <exec statement="insertTreatmentRedemptionStatement" type="insert"/>
 </exec>
 <exec type="guard">
  <conditions>
   <condition lhsfield="IS_NEW_TREATMENT" operator="&lt;&gt;" rhsvalue="true"/>
  </conditions>
  <exec recordcount="" statement="updateTreatmentRedemptionStatement" type="update"/>
 </exec>
 <exec recordcount="UPDATE_COUNT" statement="updateAccountRedemptionStatement" type="update"/>
 <exec type="guard">
  <conditions>
   <condition lhsfield="UPDATE_COUNT" operator="=" rhsvalue="0"/>
  </conditions>
  <exec statement="insertAccountRedemptionStatement" type="insert"/>
 </exec>
</event>
