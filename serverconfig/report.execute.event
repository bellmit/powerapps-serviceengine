<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec statement="getReportDesignDocumentIdForEvent" type="select"/>
 <exec throttlename="REPORT_EXECUTE" type="throttle">
  <exec type="transaction">
   <exec directToDatabase="true" directorydatepattern="yyyy-MM-dd" exportformat="XML" filenamedatepattern="HH-mm-ss-" filenameprefix="" from="DOCUMENT_ID" to="GENERATED_DOCUMENT_ID" type="generatetemplate"/>
   <exec from="ID" to="REPORT_DESIGN_ID" type="copy"/>
   <exec from="GENERATED_DOCUMENT_ID" to="DOCUMENT_ID" type="copy"/>
   <exec ceiling="false" endmonth="false" nextweekday="false" startmonth="false" to="GENERATION_DATE" truncate="false" type="date"/>
   <exec statement="insertReportInstance" type="insert"/>
  </exec>
 </exec>
</event>
