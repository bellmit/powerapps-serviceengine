<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec type="guard">
  <conditions>
   <condition lhsfield="EFFECTIVE_DATE" operator="not present" rhsvalue=""/>
  </conditions>
  <exec ceiling="false" endmonth="false" nextweekday="false" startmonth="false" to="EFFECTIVE_DATE" truncate="false" type="date"/>
 </exec>
 <exec statement="getDecisionTreeSmsBatch" type="select"/>
 <exec type="guard">
  <conditions>
   <condition lhsfield="DOCUMENT_ID" operator="is not empty" rhsvalue=""/>
  </conditions>
  <exec event="visualTree.executeSms" ignoreresult="true" type="raisewith"/>
 </exec>
</event>
