<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec argument="ACCOUNT_ATTEMPTS_SUMMARY" query="getAgencyNameForKpiAttempts" type="query"/>
 <exec type="parallelquery">
  <query value="getAgencyMettingAttempts"/>
  <query value="getAgencySmsAttempts"/>
  <query value="getAgencyLetterAttempts"/>
  <query value="getAgencyPacAttempts"/>
  <query value="getAgencyPtpAttempts"/>
  <query value="getAgencyNameForPtpSummary"/>
  <query value="getAgencyPromisesKept"/>
  <query value="getAgencyBrokenPtpAttempts"/>
  <query value="getAgencyPtpCreated"/>
  <query value="getAgencyAttemptsDetails"/>
  <target value="MEETING_ATTEMPT_LIST"/>
  <target value="SMS_ATTEMPT_LIST"/>
  <target value="LETTER_ATTEMPT_LIST"/>
  <target value="PAC_ATTEMPT_LIST"/>
  <target value="PTP_ATTEMPT_LIST"/>
  <target value="ACCOUNT_PTP_ATTEMPTS"/>
  <target value="PROMISE_KEPT_LIST"/>
  <target value="BROKEN_PROMISE_LIST"/>
  <target value="PTP_CREATED_LIST"/>
  <target value="ACCOUNT_ATTEMPTS_DETAILS"/>
 </exec>
 <exec from="LETTER_ATTEMPT_LIST" key="AGENCY_ID" to="ACCOUNT_ATTEMPTS_SUMMARY" type="merge"/>
 <exec from="MEETING_ATTEMPT_LIST" key="AGENCY_ID" to="ACCOUNT_ATTEMPTS_SUMMARY" type="merge"/>
 <exec from="PAC_ATTEMPT_LIST" key="AGENCY_ID" to="ACCOUNT_ATTEMPTS_SUMMARY" type="merge"/>
 <exec from="SMS_ATTEMPT_LIST" key="AGENCY_ID" to="ACCOUNT_ATTEMPTS_SUMMARY" type="merge"/>
 <exec from="PTP_ATTEMPT_LIST" key="AGENCY_ID" to="ACCOUNT_ATTEMPTS_SUMMARY" type="merge"/>
 <exec from="PROMISE_KEPT_LIST" key="AGENCY_ID" to="ACCOUNT_PTP_ATTEMPTS" type="merge"/>
 <exec from="BROKEN_PROMISE_LIST" key="AGENCY_ID" to="ACCOUNT_PTP_ATTEMPTS" type="merge"/>
 <exec from="PTP_CREATED_LIST" key="AGENCY_ID" to="ACCOUNT_PTP_ATTEMPTS" type="merge"/>
 <exec argument="ACCOUNT_PTP_ATTEMPTS" type="loop">
  <exec type="guard">
   <conditions>
    <condition lhsfield="PTP_CREATED" operator="&lt;&gt;" rhsvalue="0"/>
   </conditions>
   <exec lhs="PROMISE_KEPT" rhs="PTP_CREATED" target="SUCCESS_RATE" type="divide"/>
  </exec>
 </exec>
</event>
