<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec query="getPdcBank" type="query"/>
 <exec type="loop">
  <exec type="guard">
   <conditions>
    <condition lhsfield="ID" operator="is not empty" rhsvalue=""/>
   </conditions>
   <exec from="ID" to="BANK_ID" type="copy"/>
   <exec argument="BRANCH_LIST" query="getPdcBankBranch" type="query"/>
   <exec argument="BRANCH_LIST" type="loop">
    <exec from="DESCRIPTION" to="PDC_BANK_BRANCH" type="copy"/>
   </exec>
  </exec>
 </exec>
</event>
