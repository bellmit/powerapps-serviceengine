<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec count="100" to="EMPTY_LIST" type="createlist"/>
 <exec argument="SUBTYPE_LIST" type="loop">
  <exec from="ID" to="SUBTYPE_ID" type="copy"/>
  <exec from="CODE" to="SUBTYPE_CODE" type="copy"/>
 </exec>
 <exec statement="getTreatmentManagementApproval" type="select"/>
 <exec statement="getManagementApprovalSubtypeCode" type="select"/>
 <exec argument="APPENDIXB_LIST" query="getManagementApprovalAppendixB" type="query"/>
 <exec argument="APPENDIXD_LIST" query="getManagementApprovalAppendixD" type="query"/>
 <exec type="parallelquery">
  <query value="getManagementApprovalAccountStatus"/>
  <query value="getManagementApprovalCentre"/>
  <query value="getManagementApprovalActionType"/>
  <target value="ACCOUNT_STATUS_LIST"/>
  <target value="CENTRE_LIST"/>
  <target value="ACTION_TYPE_LIST"/>
 </exec>
 <exec from="ACTION_TYPE_LIST" key="SUBTYPE_ID" target="ACTION_TYPE_LIST" to="SUBTYPE_LIST" type="groupedmerge"/>
 <exec argument="OPTION_TYPE_LIST" query="getManagementApprovalOptionType" type="query"/>
 <exec argument="LEGAL_PROCESS_LIST" query="getManagementApprovalLegalProcess" type="query"/>
 <exec from="OPTION_TYPE_LIST" key="OPTION_TYPE_LIST" to="APPENDIXB_LIST" type="copyintolist"/>
 <exec from="LEGAL_PROCESS_LIST" key="LEGAL_PROCESS_LIST" to="APPENDIXD_LIST" type="copyintolist"/>
 <exec message="SHOW" target="SHOW" type="formattext"/>
 <exec message="HIDE" target="HIDE" type="formattext"/>
 <exec from="HIDE" to="appendixB" type="copy"/>
 <exec from="HIDE" to="appendixD" type="copy"/>
 <exec from="HIDE" to="blanketWaiver" type="copy"/>
 <exec from="HIDE" to="blanketLegal" type="copy"/>
 <exec type="guard">
  <conditions>
   <condition lhsfield="SUBTYPE_CODE" operator="=" rhsvalue="MA_APPXB"/>
  </conditions>
  <exec from="SHOW" to="appendixB" type="copy"/>
 </exec>
 <exec type="guard">
  <conditions>
   <condition lhsfield="SUBTYPE_CODE" operator="=" rhsvalue="MA_APPXD"/>
  </conditions>
  <exec from="SHOW" to="appendixD" type="copy"/>
 </exec>
 <exec type="guard">
  <conditions>
   <condition lhsfield="SUBTYPE_CODE" operator="=" rhsvalue="MA_BWL"/>
  </conditions>
  <exec from="SHOW" to="blanketWaiver" type="copy"/>
 </exec>
 <exec type="guard">
  <conditions>
   <condition lhsfield="SUBTYPE_CODE" operator="=" rhsvalue="MA_BLA"/>
  </conditions>
  <exec from="SHOW" to="blanketLegal" type="copy"/>
 </exec>
</event>
