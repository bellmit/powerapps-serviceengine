<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec ceiling="false" endmonth="false" nextweekday="false" startmonth="true" truncate="true" type="date"/>
 <exec ceiling="true" endmonth="true" nextweekday="false" startmonth="false" truncate="false" type="date"/>
 <exec statement="getCamOutlookDetails" type="select"/>
 <exec text="Declass/Payment" type="comment">
  <exec type="guard">
   <conditions>
    <condition lhsfield="ACCOUNT_TYPE" operator="=" rhsvalue="NPF"/>
   </conditions>
   <exec type="guard">
    <conditions>
     <condition lhsfield="MTD_PAYMENT_GREATER_THAN_ZERO" operator="=" rhsvalue="true"/>
    </conditions>
    <exec type="guard">
     <conditions>
      <condition lhsfield="NPL_STATUS" operator="=" rhsvalue="Y"/>
     </conditions>
     <exec message="P" target="DECLASS_PAYMENT" type="formattext"/>
    </exec>
    <exec type="guard">
     <conditions>
      <condition lhsfield="NPL_STATUS" operator="=" rhsvalue="N"/>
     </conditions>
     <exec message="D" target="DECLASS_PAYMENT" type="formattext"/>
    </exec>
   </exec>
   <exec type="guard">
    <conditions>
     <condition lhsfield="MTD_PAYMENT_GREATER_THAN_ZERO" operator="=" rhsvalue="false"/>
    </conditions>
    <exec message="No" target="DECLASS_PAYMENT" type="formattext"/>
   </exec>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="ACCOUNT_TYPE" operator="=" rhsvalue="BDR"/>
   </conditions>
   <exec type="guard">
    <conditions>
     <condition lhsfield="MTD_PAYMENT_GREATER_THAN_ZERO" operator="=" rhsvalue="true"/>
    </conditions>
    <exec message="P" target="DECLASS_PAYMENT" type="formattext"/>
   </exec>
   <exec type="guard">
    <conditions>
     <condition lhsfield="MTD_PAYMENT_GREATER_THAN_ZERO" operator="=" rhsvalue="false"/>
    </conditions>
    <exec message="No" target="DECLASS_PAYMENT" type="formattext"/>
   </exec>
  </exec>
 </exec>
 <exec text="Actual Declass Payment" type="comment">
  <exec type="guard">
   <conditions>
    <condition lhsfield="ACCOUNT_TYPE" operator="=" rhsvalue="NPF"/>
   </conditions>
   <exec type="guard">
    <conditions>
     <condition lhsfield="MTD_PAYMENT_GREATER_THAN_ZERO" operator="=" rhsvalue="true"/>
    </conditions>
    <exec type="guard">
     <conditions>
      <condition lhsfield="NPL_STATUS" operator="=" rhsvalue="Y"/>
     </conditions>
     <exec message="P" target="DECLASS_PAYMENT" type="formattext"/>
     <exec from="MTD_PAYMENT" to="ACTUAL_DECLASS_PAYMENT" type="copy"/>
    </exec>
    <exec type="guard">
     <conditions>
      <condition lhsfield="NPL_STATUS" operator="=" rhsvalue="N"/>
     </conditions>
     <exec message="D" target="DECLASS_PAYMENT" type="formattext"/>
     <exec from="OPENING_NLB" to="ACTUAL_DECLASS_PAYMENT" type="copy"/>
    </exec>
   </exec>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="ACCOUNT_TYPE" operator="=" rhsvalue="BDR"/>
   </conditions>
   <exec from="MTD_PAYMENT" to="ACTUAL_DECLASS_PAYMENT" type="copy"/>
  </exec>
 </exec>
 <exec text="Net Amount" type="comment">
  <exec type="guard">
   <conditions>
    <condition lhsfield="MTD_PAYMENT_GREATER_OR_EQUAL_THAN_REVISED_AMOUNT" operator="=" rhsvalue="true"/>
   </conditions>
   <exec from="ZERO" to="NET_AMOUNT" type="copy"/>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="MTD_PAYMENT_GREATER_OR_EQUAL_THAN_REVISED_AMOUNT" operator="&lt;&gt;" rhsvalue="true"/>
   </conditions>
   <exec lhs="MTD_PAYMENT" rhs="NEGATIVE_ONE" target="NEGATIVE_MTD_PAYMENT" type="multiply"/>
   <exec lhs="REVISED_AMOUNT" rhs="NEGATIVE_MTD_PAYMENT" target="NET_AMOUNT" type="add"/>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="ACCOUNT_TYPE" operator="&lt;&gt;" rhsvalue="BDR"/>
   </conditions>
   <exec type="guard">
    <conditions>
     <condition lhsfield="DECLASS_PAYMENT" operator="=" rhsvalue="D"/>
    </conditions>
    <exec from="ZERO" to="NET_AMOUNT" type="copy"/>
   </exec>
  </exec>
 </exec>
 <exec text="Outlook Amount" type="comment">
  <exec type="guard">
   <conditions>
    <condition lhsfield="IS_TREATMENT_EXIST" operator="=" rhsvalue="true"/>
   </conditions>
   <exec from="REVISED_AMOUNT" to="OUTLOOK" type="copy"/>
   <exec type="guard">
    <conditions>
     <condition lhsfield="ACCOUNT_TYPE" operator="=" rhsvalue="NPF"/>
    </conditions>
    <exec type="guard">
     <conditions>
      <condition lhsfield="DECLASS_PAYMENT" operator="=" rhsvalue="P"/>
     </conditions>
     <exec type="guard">
      <conditions>
       <condition lhsfield="MTD_PAYMENT_GREATER_OR_EQUAL_THAN_REVISED_AMOUNT" operator="=" rhsvalue="true"/>
      </conditions>
      <exec from="MTD_PAYMENT" to="OUTLOOK" type="copy"/>
     </exec>
     <exec type="guard">
      <conditions>
       <condition lhsfield="MTD_PAYMENT_GREATER_OR_EQUAL_THAN_REVISED_AMOUNT" operator="&lt;&gt;" rhsvalue="true"/>
      </conditions>
      <exec type="guard">
       <conditions>
        <condition lhsfield="CONFIDENCE_LEVEL" operator="=" rhsvalue="C"/>
       </conditions>
       <exec lhs="NET_AMOUNT" rhs="PERCENTAGE_90" target="CONFIDENCE_NET_AMOUNT" type="multiply"/>
      </exec>
      <exec type="guard">
       <conditions>
        <condition lhsfield="CONFIDENCE_LEVEL" operator="=" rhsvalue="PC"/>
       </conditions>
       <exec lhs="NET_AMOUNT" rhs="PERCENTAGE_50" target="CONFIDENCE_NET_AMOUNT" type="multiply"/>
      </exec>
      <exec statement="getOutlookAmount" type="select"/>
     </exec>
    </exec>
    <exec type="guard">
     <conditions>
      <condition lhsfield="DECLASS_PAYMENT" operator="=" rhsvalue="No"/>
     </conditions>
     <exec type="guard">
      <conditions>
       <condition lhsfield="MTD_PAYMENT_GREATER_OR_EQUAL_THAN_REVISED_AMOUNT" operator="=" rhsvalue="true"/>
      </conditions>
      <exec from="MTD_PAYMENT" to="OUTLOOK" type="copy"/>
     </exec>
     <exec type="guard">
      <conditions>
       <condition lhsfield="MTD_PAYMENT_GREATER_OR_EQUAL_THAN_REVISED_AMOUNT" operator="&lt;&gt;" rhsvalue="true"/>
      </conditions>
      <exec type="guard">
       <conditions>
        <condition lhsfield="CONFIDENCE_LEVEL" operator="=" rhsvalue="C"/>
       </conditions>
       <exec lhs="NET_AMOUNT" rhs="PERCENTAGE_90" target="CONFIDENCE_NET_AMOUNT" type="multiply"/>
      </exec>
      <exec type="guard">
       <conditions>
        <condition lhsfield="CONFIDENCE_LEVEL" operator="=" rhsvalue="PC"/>
       </conditions>
       <exec lhs="NET_AMOUNT" rhs="PERCENTAGE_50" target="CONFIDENCE_NET_AMOUNT" type="multiply"/>
      </exec>
      <exec statement="getOutlookAmount" type="select"/>
     </exec>
    </exec>
    <exec type="guard">
     <conditions>
      <condition lhsfield="DECLASS_PAYMENT" operator="=" rhsvalue="D"/>
     </conditions>
     <exec from="OPENING_NLB" to="OUTLOOK" type="copy"/>
    </exec>
   </exec>
   <exec type="guard">
    <conditions>
     <condition lhsfield="ACCOUNT_TYPE" operator="=" rhsvalue="BDR"/>
    </conditions>
    <exec type="guard">
     <conditions>
      <condition lhsfield="MTD_PAYMENT_GREATER_OR_EQUAL_THAN_REVISED_AMOUNT" operator="=" rhsvalue="true"/>
     </conditions>
     <exec from="MTD_PAYMENT" to="OUTLOOK" type="copy"/>
    </exec>
   </exec>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="IS_TREATMENT_EXIST" operator="&lt;&gt;" rhsvalue="true"/>
   </conditions>
   <exec type="guard">
    <conditions>
     <condition lhsfield="DECLASS_PAYMENT" operator="=" rhsvalue="D"/>
    </conditions>
    <exec from="OPENING_NLB" to="OUTLOOK" type="copy"/>
   </exec>
   <exec type="guard">
    <conditions>
     <condition lhsfield="DECLASS_PAYMENT" operator="=" rhsvalue="P"/>
    </conditions>
    <exec from="MTD_PAYMENT" to="OUTLOOK" type="copy"/>
   </exec>
  </exec>
 </exec>
</event>
