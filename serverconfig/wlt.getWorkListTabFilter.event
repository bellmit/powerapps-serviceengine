    <condition lhsfield="TYPE_CODE" operator="=" rhsvalue="bankuser"/>
   </conditions>
   <exec argument="HP_WLT_LIST" query="getWorkListTabsReferenceHp" type="query"/>
  </exec>
  <exec from="HP_WLT_LIST" to="RESULT_LIST" type="copy"/>
