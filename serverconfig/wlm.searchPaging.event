<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec type="guard">
  <conditions>
   <condition lhsfield="PAGING_CONDITION" operator="=" rhsvalue="getPagingArguments"/>
  </conditions>
  <exec statement="getWorkListManagerSearchArgs" type="select"/>
  <exec type="guard">
   <conditions>
    <condition lhsfield="SEARCH" operator="=" rhsvalue="true"/>
   </conditions>
   <exec from="ONE" to="PAGE_NUMBER" type="copy"/>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="NEXT_PAGE" operator="=" rhsvalue="true"/>
   </conditions>
   <exec type="guard">
    <conditions>
     <condition lhsfield="PAGE_NUMBER" operator="is empty" rhsvalue=""/>
    </conditions>
    <exec from="ONE" to="PAGE_NUMBER" type="copy"/>
   </exec>
   <exec lhs="PAGE_NUMBER" rhs="ONE" target="PAGE_NUMBER" type="add"/>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="PREVIOUS_PAGE" operator="=" rhsvalue="true"/>
   </conditions>
   <exec lhs="PAGE_NUMBER" rhs="NEGATIVE_ONE" target="PAGE_NUMBER" type="add"/>
  </exec>
 </exec>
 <exec type="guard">
  <conditions>
   <condition lhsfield="PAGING_CONDITION" operator="=" rhsvalue="getPagingProcess"/>
  </conditions>
  <exec type="guard">
   <conditions>
    <condition lhsfield="TOTAL_PAGE_COUNT" operator="=" rhsvalue="1"/>
   </conditions>
   <exec from="TRUE" to="DISABLE_PREV" type="copy"/>
   <exec from="TRUE" to="DISABLE_NEXT" type="copy"/>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="TOTAL_PAGE_COUNT" operator="&lt;&gt;" rhsvalue="1"/>
   </conditions>
   <exec type="guard">
    <conditions>
     <condition lhsfield="PAGE_NUMBER" operator="=" rhsvalue="1"/>
    </conditions>
    <exec from="TRUE" to="DISABLE_PREV" type="copy"/>
    <exec from="FALSE" to="DISABLE_NEXT" type="copy"/>
   </exec>
   <exec type="guard">
    <conditions>
     <condition lhsfield="PAGE_NUMBER" operator="&lt;&gt;" rhsvalue="1"/>
    </conditions>
    <exec lhs="PAGE_NUMBER" rhs="NEGATIVE_ONE" target="NEGATIVE_PAGE_NUMBER" type="multiply"/>
    <exec lhs="TOTAL_PAGE_COUNT" rhs="NEGATIVE_PAGE_NUMBER" target="END_PAGE" type="add"/>
    <exec from="FALSE" to="DISABLE_PREV" type="copy"/>
    <exec type="guard">
     <conditions>
      <condition lhsfield="TOTAL_PAGE_COUNT" operator="equal to value of" rhsvalue="PAGE_NUMBER"/>
     </conditions>
     <exec from="TRUE" to="DISABLE_NEXT" type="copy"/>
    </exec>
   </exec>
  </exec>
 </exec>
</event>
