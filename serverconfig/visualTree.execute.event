<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec count="1" type="createlist"/>
 <exec from="TRUE" key="TRUE" type="copyintolist"/>
 <exec from="FALSE" key="FALSE" type="copyintolist"/>
 <exec from="TREE_ID" key="TREE_ID" type="copyintolist"/>
 <exec from="TREE_TYPE_ID" key="TREE_TYPE_ID" type="copyintolist"/>
 <exec from="CONFIRM_UPDATE" key="CONFIRM_UPDATE" type="copyintolist"/>
 <exec type="loop">
  <exec from="TRUE" to="OPEN_MESSAGE" type="copy"/>
  <exec type="guard">
   <conditions>
    <condition lhsfield="CONFIRM_UPDATE" operator="&lt;&gt;" rhsvalue="true"/>
   </conditions>
   <exec message="Action Cancelled" target="MESSAGE" type="formattext"/>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="CONFIRM_UPDATE" operator="=" rhsvalue="true"/>
   </conditions>
   <exec message="Tree Executed" target="MESSAGE" type="formattext"/>
   <exec ceiling="false" endmonth="false" nextweekday="false" startmonth="false" to="EFFECTIVE_DATE" truncate="false" type="date"/>
   <exec statement="getDecisionTreeDocumentId" type="select"/>
   <exec type="guard">
    <conditions>
     <condition lhsfield="TREE_TYPE" operator="=" rhsvalue="Visual SMS"/>
    </conditions>
    <exec event="visualTree.executeSms" ignoreresult="true" type="raisewith"/>
   </exec>
   <exec type="guard">
    <conditions>
     <condition lhsfield="TREE_TYPE" operator="=" rhsvalue="Visual Letter"/>
    </conditions>
    <exec event="visualTree.executeLetter" ignoreresult="true" type="raisewith"/>
   </exec>
   <exec type="guard">
    <conditions>
     <condition lhsfield="TREE_TYPE" operator="=" rhsvalue="Visual Visitation"/>
    </conditions>
    <exec event="visualTree.executeVisitation" ignoreresult="true" type="raisewith"/>
   </exec>
  </exec>
 </exec>
</event>
