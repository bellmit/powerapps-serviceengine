<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec type="guard">
  <conditions>
   <condition lhsfield="IS_NEW_TREATMENT" operator="=" rhsvalue="true"/>
  </conditions>
  <exec statement="insertTreatmentInitiateRnr" type="insert"/>
 </exec>
 <exec type="guard">
  <conditions>
   <condition lhsfield="IS_NEW_TREATMENT" operator="&lt;&gt;" rhsvalue="true"/>
  </conditions>
  <exec recordcount="" statement="updateTreatmentInitiateRnr" type="update"/>
 </exec>
 <exec statement="deleteTreatmentInitiateRnrPrerequisite" type="delete"/>
 <exec message="T1" target="T1" type="formattext"/>
 <exec message="T2" target="T2" type="formattext"/>
 <exec from="ID" key="TREATMENT_ID" to="PREREQUISITE_TABLE_1" type="copyintolist"/>
 <exec from="T1" key="TABLE_CODE" to="PREREQUISITE_TABLE_1" type="copyintolist"/>
 <exec from="ID" key="TREATMENT_ID" to="PREREQUISITE_TABLE_2" type="copyintolist"/>
 <exec from="T2" key="TABLE_CODE" to="PREREQUISITE_TABLE_2" type="copyintolist"/>
 <exec argument="PREREQUISITE_TABLE_1" type="loop">
  <exec type="guard">
   <conditions>
    <condition lhsfield="ACCOUNT_ID" operator="is not empty" rhsvalue=""/>
   </conditions>
   <exec statement="insertTreatmentInitiateRnrPrerequisite" type="insert"/>
  </exec>
 </exec>
 <exec argument="PREREQUISITE_TABLE_2" type="loop">
  <exec type="guard">
   <conditions>
    <condition lhsfield="ACCOUNT_ID" operator="is not empty" rhsvalue=""/>
   </conditions>
   <exec statement="insertTreatmentInitiateRnrPrerequisite" type="insert"/>
  </exec>
 </exec>
</event>
