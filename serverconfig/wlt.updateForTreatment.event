<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec text="Collate args" type="comment">
  <exec ceiling="false" endmonth="false" nextweekday="false" startmonth="false" truncate="true" type="date"/>
  <exec from="CURRENT_DATE" to="UPDATED_TIME" type="copy"/>
  <exec from="CURRENT_DATE" to="ACTUAL_END_DATE" type="copy"/>
  <exec type="guard">
   <conditions>
    <condition lhsfield="APPOINTMENT_TIME" operator="exists" rhsvalue=""/>
   </conditions>
   <exec from="APPOINTMENT_TIME" to="FOLLOW_UP_DATE" type="copy"/>
  </exec>
  <exec from="ID" to="TREATMENT_PROCESS_ID" type="copy"/>
  <exec from="IS_NEW_TREATMENT" to="NEW_TREATMENT" type="copy"/>
  <exec from="TYPE_STATUS_ID" to="PROCESS_TYPE_STATUS_ID" type="copy"/>
  <exec from="STATUS_ID" to="PROCESS_STATUS_ID" type="copy"/>
  <exec from="SUBTYPE_ID" to="PROCESS_SUBTYPE_ID" type="copy"/>
  <exec from="TYPE_ID" to="PROCESS_TYPE_ID" type="copy"/>
  <exec statement="getWltTreatmentStatusCode" type="select"/>
  <exec recordcount="UPDATE_WLT_COUNT" statement="updateWorkListTabEvaluation" type="update"/>
  <exec type="guard">
   <conditions>
    <condition lhsfield="UPDATE_WLT_COUNT" operator="=" rhsvalue="0"/>
   </conditions>
   <exec statement="insertWorkListTabEvaluation" type="insert"/>
  </exec>
 </exec>
 <exec event="wlt.getUserType" ignoreresult="true" type="raise"/>
 <exec type="guard">
  <conditions>
   <condition lhsfield="TYPE_CODE" operator="=" rhsvalue="agencyuser"/>
  </conditions>
  <exec statement="getAssignmentIdForTreatmentCall" type="select"/>
 </exec>
 <exec text="Place a call / Work list tabs evaluation" type="comment">
  <exec type="guard">
   <conditions>
    <condition lhsfield="PROCESS_TYPE_ID" operator="=" rhsvalue="8"/>
   </conditions>
   <exec text="Check lost contact" type="comment">
    <exec from="FALSE" to="IS_LOST_CONTACT" type="copy"/>
    <exec type="guard">
     <conditions>
      <condition lhsfield="PROCESS_STATUS_CODE" operator="=" rhsvalue="PLACE_UNS_AND"/>
     </conditions>
     <exec from="TRUE" to="IS_LOST_CONTACT" type="copy"/>
    </exec>
    <exec type="guard">
     <conditions>
      <condition lhsfield="PROCESS_STATUS_CODE" operator="=" rhsvalue="REVIEW_4_AGENCY"/>
     </conditions>
     <exec from="TRUE" to="IS_LOST_CONTACT" type="copy"/>
    </exec>
   </exec>
   <exec recordcount="WLT_UPDATE_COUNT" statement="updateCallWorkListTabs" type="update"/>
   <exec type="guard">
    <conditions>
     <condition lhsfield="WLT_UPDATE_COUNT" operator="=" rhsvalue="0"/>
    </conditions>
    <exec statement="insertWorkListTabEvaluation" type="insert"/>
    <exec recordcount="WLT_UPDATE_COUNT" statement="updateCallWorkListTabs" type="update"/>
   </exec>
  </exec>
 </exec>
 <exec text="PTP Work list tabs evaluation" type="comment">
  <exec type="guard">
   <conditions>
    <condition lhsfield="PROCESS_TYPE_ID" operator="=" rhsvalue="2"/>
   </conditions>
   <exec recordcount="WLT_UPDATE_COUNT" statement="updateWorkListTabEvaluationForPromise" type="update"/>
   <exec type="guard">
    <conditions>
     <condition lhsfield="WLT_UPDATE_COUNT" operator="=" rhsvalue="0"/>
    </conditions>
    <exec statement="insertWorkListTabEvaluation" type="insert"/>
    <exec recordcount="WLT_UPDATE_COUNT" statement="updateWorkListTabEvaluationForPromise" type="update"/>
   </exec>
  </exec>
 </exec>
