<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec to="LOGON_USER" type="user"/>
 <exec argument="USER_SETTING" query="getCamUserSettings" type="query"/>
 <exec type="guard">
  <conditions>
   <condition lhsfield="USER_SETTING" operator="is not empty" rhsvalue=""/>
  </conditions>
  <exec list="USER_SETTING" name="CODE" type="pivotlist" value="VALUE"/>
  <exec type="loop">
   <exec message="SHOW" target="_MENU" type="formattext"/>
   <exec message="HIDE" target="_NAV" type="formattext"/>
   <exec type="guard">
    <conditions>
     <condition lhsfield="CAM_NAV_DISPLAY" operator="=" rhsvalue="true"/>
    </conditions>
    <exec message="SHOW" target="_NAV" type="formattext"/>
    <exec message="HIDE" target="_MENU" type="formattext"/>
   </exec>
  </exec>
 </exec>
 <exec type="guard">
  <conditions>
   <condition lhsfield="USER_SETTING" operator="is empty" rhsvalue=""/>
  </conditions>
  <exec count="1" type="createlist"/>
 </exec>
 <exec message="HIDE" target="HIDE" type="formattext"/>
 <exec message="SHOW" target="SHOW" type="formattext"/>
 <exec from="SHOW" to="_CUSTOMER" type="copy"/>
 <exec from="SHOW" to="_ACCOUNT" type="copy"/>
 <exec from="SHOW" to="_COLLATERAL" type="copy"/>
 <exec from="SHOW" to="_HISTORY" type="copy"/>
 <exec from="_CUSTOMER" key="_CUSTOMER" type="copyintolist"/>
 <exec from="_ACCOUNT" key="_ACCOUNT" type="copyintolist"/>
 <exec from="_COLLATERAL" key="_COLLATERAL" type="copyintolist"/>
 <exec from="_HISTORY" key="_HISTORY" type="copyintolist"/>
 <exec count="12" to="ALL_NAVIGATIONS" type="createlist"/>
 <exec count="1" to="NAVIGATIONS" type="createlist"/>
 <exec argument="NAVIGATIONS" type="loop">
  <exec message=" " target="DESCRIPTION" type="formattext"/>
  <exec from="DESCRIPTION" to="HEADER_TITLE" type="copy"/>
 </exec>
 <exec from="TRUE" key="LOAD_PAGE" to="ALL_NAVIGATIONS" type="copyintolist"/>
 <exec argument="ALL_NAVIGATIONS" index="ID" type="loop">
  <exec message="images/customer_24.png" target="ICON" type="formattext"/>
  <exec message="HIDE" target="HIDE" type="formattext"/>
  <exec message="SHOW" target="SHOW" type="formattext"/>
  <exec from="HIDE" to="_REMINDER_HISTORY" type="copy"/>
  <exec from="HIDE" to="_ACTION_TAKEN_HISTORY" type="copy"/>
  <exec type="guard">
   <conditions>
    <condition lhsfield="ID" operator="=" rhsvalue="1"/>
   </conditions>
   <exec message="Customer &gt; Account Summary" target="DESCRIPTION" type="formattext"/>
   <exec message="1" target="CODE" type="formattext"/>
   <exec message="images/customer_24.png" target="ICON" type="formattext"/>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="ID" operator="=" rhsvalue="2"/>
   </conditions>
   <exec message="Customer &gt; Customer Information" target="DESCRIPTION" type="formattext"/>
   <exec message="2" target="CODE" type="formattext"/>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="ID" operator="=" rhsvalue="3"/>
   </conditions>
   <exec message="Customer &gt; Contact Information" target="DESCRIPTION" type="formattext"/>
   <exec message="3" target="CODE" type="formattext"/>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="ID" operator="=" rhsvalue="4"/>
   </conditions>
   <exec message="Customer &gt; Statistics" target="DESCRIPTION" type="formattext"/>
   <exec message="4" target="CODE" type="formattext"/>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="ID" operator="=" rhsvalue="5"/>
   </conditions>
   <exec message="Customer &gt; Payments" target="DESCRIPTION" type="formattext"/>
   <exec message="5" target="CODE" type="formattext"/>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="ID" operator="=" rhsvalue="6"/>
   </conditions>
   <exec message="Accounts &gt; Financial Information" target="DESCRIPTION" type="formattext"/>
   <exec message="6" target="CODE" type="formattext"/>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="ID" operator="=" rhsvalue="7"/>
   </conditions>
   <exec message="Accounts &gt; Loan Inquiry " target="DESCRIPTION" type="formattext"/>
   <exec message="7" target="CODE" type="formattext"/>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="ID" operator="=" rhsvalue="8"/>
   </conditions>
   <exec message="Accounts &gt; Account Relationships " target="DESCRIPTION" type="formattext"/>
   <exec message="8" target="CODE" type="formattext"/>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="ID" operator="=" rhsvalue="9"/>
   </conditions>
   <exec message="Accounts &gt; Rate &amp; Status Change History" target="DESCRIPTION" type="formattext"/>
   <exec message="9" target="CODE" type="formattext"/>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="ID" operator="=" rhsvalue="10"/>
   </conditions>
   <exec message="Accounts &gt; Financial Information" target="DESCRIPTION" type="formattext"/>
   <exec message="10" target="CODE" type="formattext"/>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="ID" operator="=" rhsvalue="11"/>
   </conditions>
   <exec message="History &gt; Action Taken History" target="DESCRIPTION" type="formattext"/>
   <exec message="11" target="CODE" type="formattext"/>
   <exec from="SHOW" to="_ACTION_TAKEN_HISTORY" type="copy"/>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="ID" operator="=" rhsvalue="12"/>
   </conditions>
   <exec message="History &gt; Reminder History" target="DESCRIPTION" type="formattext"/>
   <exec message="12" target="CODE" type="formattext"/>
   <exec from="SHOW" to="_REMINDER_HISTORY" type="copy"/>
  </exec>
  <exec from="DESCRIPTION" to="HEADER_TITLE" type="copy"/>
 </exec>
 <exec from="ALL_NAVIGATIONS" to="NAVIGATIONS" type="append"/>
 <exec from="NAVIGATIONS" key="NAVIGATIONS" type="copyintolist"/>
</event>
