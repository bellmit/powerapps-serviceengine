<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="true" type="event">
 <exec to="LOGON_USER" type="user"/>
 <exec ceiling="false" endmonth="false" nextweekday="false" startmonth="false" truncate="false" type="date"/>
 <exec text="Update PowerCollect numbers" type="comment">
  <exec type="guard">
   <conditions>
    <condition lhsfield="SOURCE_INDICATOR" operator="&lt;&gt;" rhsvalue="host"/>
   </conditions>
   <exec type="guard">
    <conditions>
     <condition lhsfield="CONTACT_SOURCE_ID" operator="is empty" rhsvalue=""/>
    </conditions>
    <exec statement="getContactSourcePowerCollect" type="select"/>
   </exec>
   <exec statement="getContactNumberCurrentVerifiedStatus" type="select"/>
   <exec statement="getContactNumberIsReactivated" type="select"/>
   <exec type="guard">
    <conditions>
     <condition lhsfield="CONTACT_ID" operator="is not empty" rhsvalue=""/>
    </conditions>
    <exec recordcount="" statement="updateContactNumber" type="update"/>
    <exec type="guard">
     <conditions>
      <condition lhsfield="IS_REACTIVATED" operator="=" rhsvalue="true"/>
     </conditions>
     <exec recordcount="" statement="updateContactNumberIsReactivated" type="update"/>
    </exec>
   </exec>
   <exec type="guard">
    <conditions>
     <condition lhsfield="CONTACT_ID" operator="is empty" rhsvalue=""/>
    </conditions>
    <exec statement="insertContactNumber" type="insert"/>
    <exec from="ID" to="CONTACT_ID" type="copy"/>
   </exec>
   <exec type="guard">
    <conditions>
     <condition lhsfield="IS_CURRENT_VERIFIED_STATUS" operator="&lt;&gt;" rhsvalue="true"/>
     <condition lhsfield="VERIFIED_STATUS_ID" operator="is not empty" rhsvalue=""/>
    </conditions>
    <exec recordcount="" statement="updateContactNumberVerifiedStatus" type="update"/>
   </exec>
  </exec>
 </exec>
 <exec text="Update host numbers" type="comment">
  <exec type="guard">
   <conditions>
    <condition lhsfield="SOURCE_INDICATOR" operator="=" rhsvalue="host"/>
   </conditions>
   <exec recordcount="" statement="updateContactNumberHost" type="update"/>
  </exec>
 </exec>
 <exec count="1" type="createlist"/>
 <exec from="TRUE" key="CANCEL" type="copyintolist"/>
</event>
