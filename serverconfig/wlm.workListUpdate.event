<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec event="wlm.getUserType" ignoreresult="true" type="raise"/>
 <exec to="USER_ID" type="user"/>
 <exec ceiling="false" endmonth="false" nextweekday="false" startmonth="false" to="UPDATE_TIME" truncate="false" type="date"/>
 <exec from="SELECTED_WORK_LIST_ID" to="WORK_LIST_ID" type="copy"/>
 <exec from="WLM_WORKLIST_EDIT_WORKLIST_NAME" to="WORK_LIST_NAME" type="copy"/>
 <exec from="WLM_WORKLIST_EDIT_WORKLIST_DESC" to="DESCRIPTION" type="copy"/>
 <exec from="WORK_LIST_CATEGORY_ID" to="WORK_LIST_CATEGORY_ID" type="copy"/>
 <exec from="EXTRACT_TO_DIALER_EDIT" to="EXTRACT_TO_DIALER" type="copy"/>
 <exec from="BRANCH_CENTRE_ID_EDIT" to="BRANCH_CENTRE_ID" type="copy"/>
 <exec type="transaction">
  <exec type="guard">
   <conditions>
    <condition lhsfield="MODULE_NAME" operator="=" rhsvalue="RNR"/>
   </conditions>
   <exec recordcount="" statement="updateRnRWorkList" type="update"/>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="MODULE_NAME" operator="&lt;&gt;" rhsvalue="RNR"/>
   </conditions>
   <exec type="guard">
    <conditions>
     <condition lhsfield="TYPE_CODE" operator="=" rhsvalue="agencyuser"/>
    </conditions>
    <exec recordcount="" statement="updateAgencyWorkList" type="update"/>
   </exec>
   <exec type="guard">
    <conditions>
     <condition lhsfield="TYPE_CODE" operator="=" rhsvalue="bankuser"/>
    </conditions>
    <exec recordcount="" statement="updateWorkList" type="update"/>
   </exec>
  </exec>
 </exec>
 <exec event="wlm.getWlmWorkLists" ignoreresult="true" type="raise"/>
 <exec from="WORK_LIST_INDX" key="WORK_LIST_INDX" to="RESULT_LIST" type="copyintolist"/>
 <exec from="WLM_TYPE_DESC" key="WLM_TYPE_DESC" to="RESULT_LIST" type="copyintolist"/>
 <exec from="RESULT_LIST" type="copy"/>
</event>
