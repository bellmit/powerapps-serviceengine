<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec text="Verify arguments present" type="comment">
  <exec type="guard">
   <conditions>
    <condition lhsfield="PROCESS_NAME" operator="not present" rhsvalue=""/>
   </conditions>
   <exec code="MISSING_HOST_PROCESSING_PARAMETER" type="error"/>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="EFFECTIVE_DATE" operator="not present" rhsvalue=""/>
   </conditions>
   <exec code="MISSING_HOST_PROCESSING_PARAMETER" type="error"/>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="MAXIMUM_LOAD_EXECUTION_ID" operator="not present" rhsvalue=""/>
   </conditions>
   <exec code="MISSING_HOST_PROCESSING_PARAMETER" type="error"/>
  </exec>
 </exec>
 <exec ceiling="false" endmonth="false" nextweekday="false" startmonth="false" to="START_TIME" truncate="false" type="date"/>
