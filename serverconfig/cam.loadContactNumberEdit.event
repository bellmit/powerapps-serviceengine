<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec count="1" type="createlist"/>
 <exec from="CONTACT_ID" key="CONTACT_ID" type="copyintolist"/>
 <exec from="TRUE" key="TRUE" type="copyintolist"/>
 <exec from="FALSE" key="FALSE" type="copyintolist"/>
 <exec type="guard">
  <conditions>
   <condition lhsfield="SOURCE_INDICATOR" operator="&lt;&gt;" rhsvalue="host"/>
  </conditions>
  <exec type="loop">
   <exec type="guard">
    <conditions>
     <condition lhsfield="CONTACT_ID" operator="is not empty" rhsvalue=""/>
     <condition lhsfield="CONTACT_ID" operator="&lt;&gt;" rhsvalue="parent.NUMBER_CONTACT_ID"/>
    </conditions>
    <exec statement="getContactNumberEdit" type="select"/>
    <exec type="guard">
     <conditions>
      <condition lhsfield="SOURCE" operator="=" rhsvalue="RAMCI"/>
     </conditions>
     <exec from="TRUE" to="disableFields" type="copy"/>
    </exec>
   </exec>
   <exec argument="TYPE_LIST" query="getContactNumberTypeRef" type="query"/>
   <exec argument="VERIFIED_STATUS_LIST" query="getContactVerifiedStatus" type="query"/>
   <exec argument="TYPE_LIST" type="loop">
    <exec message="Contact No." target="TYPE" type="formattext"/>
    <exec type="guard">
     <conditions>
      <condition lhsfield="ID" operator="=" rhsvalue="4"/>
     </conditions>
     <exec message="Email Address" target="TYPE" type="formattext"/>
    </exec>
   </exec>
  </exec>
 </exec>
 <exec type="guard">
  <conditions>
   <condition lhsfield="SOURCE_INDICATOR" operator="=" rhsvalue="host"/>
  </conditions>
  <exec type="loop">
   <exec from="TRUE" to="DISABLE_ON_EDIT" type="copy"/>
   <exec type="guard">
    <conditions>
     <condition lhsfield="CONTACT_ID" operator="is not empty" rhsvalue=""/>
     <condition lhsfield="CONTACT_ID" operator="&lt;&gt;" rhsvalue="parent.NUMBER_CONTACT_ID"/>
    </conditions>
    <exec statement="getContactNumberEditHost" type="select"/>
   </exec>
   <exec argument="TYPE_LIST" query="getContactNumberTypeRefHost" type="query"/>
   <exec argument="VERIFIED_STATUS_LIST" query="getContactVerifiedStatus" type="query"/>
   <exec argument="TYPE_LIST" type="loop">
    <exec message="Contact No." target="TYPE" type="formattext"/>
   </exec>
  </exec>
 </exec>
</event>
