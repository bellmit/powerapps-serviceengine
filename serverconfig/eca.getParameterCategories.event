<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec argument="CATEGORIES" query="getEcaParameterCategories" type="query"/>
 <exec argument="CATEGORIES" index="INDEX" type="loop">
  <exec message="{0} - {1}" target="DESCRIPTION" type="formattext">
   <key value="INDEX"/>
   <key value="DESCRIPTION"/>
  </exec>
  <exec from="DESCRIPTION" to="BASE_HEADER" type="copy"/>
  <exec from="DESCRIPTION" to="HEADER" type="copy"/>
  <exec message="HIDE" target="_CUSTOM_EDITOR" type="formattext"/>
  <exec message="HIDE" target="_STANDARD_EDITOR" type="formattext"/>
  <exec message="SHOW" target="_DEFAULT" type="formattext"/>
 </exec>
 <exec from="CATEGORIES" type="copy"/>
</event>
