<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
   <exec to="USER_ID" type="user"/>
   <exec argument="USER_INFO" query="getUserUserType" type="query"/>
   <exec statement="getUserUserType" type="select"/>
   <exec type="guard">
      <conditions>
         <condition lhsfield="AGENCY_CODE" operator="is not empty" rhsvalue=""/>
      </conditions>
      <exec from="USER_INFO" key="AGENCY_ID" to="AGENCY_IDS" type="extract"/>
   </exec>
   <exec count="1" type="createlist"/>
   <exec count="5" to="CATEGORIES" type="createlist"/>
   <exec count="5" to="ACCOUNT_FILTERS" type="createlist"/>
   <exec count="3" to="PERIODS" type="createlist"/>
   <exec type="parallelquery">
      <query value="getEcaAgencyTypes"/>
      <query value="getEcaAgenciesForKpi"/>
      <query value="getProductCategoriesForECAKPI"/>
      <target value="TIER_1"/>
      <target value="TIER_2"/>
      <target value="PRODUCT_CATEGORIES"/>
   </exec>
   <exec argument="TIER_1" type="loop">
      <exec from="ID" to="TYPE_ID" type="copy"/>
      <exec from="ID" to="LEVEL_1" type="copy"/>
      <exec from="DESCRIPTION" to="LEVEL_1_DESCRIPTION" type="copy"/>
   </exec>
   <exec argument="TIER_2" type="loop">
      <exec from="ID" to="AGENCY_ID" type="copy"/>
      <exec from="TYPE_ID" to="LEVEL_1" type="copy"/>
      <exec from="ID" to="LEVEL_2" type="copy"/>
      <exec from="DESCRIPTION" to="LEVEL_2_DESCRIPTION" type="copy"/>
   </exec>
   <exec from="TIER_1" to="TREE_LIST" type="append"/>
   <exec from="TIER_2" to="TREE_LIST" type="append"/>
   <exec argument="CATEGORIES" index="ID" type="loop">
      <exec message="HIDE" target="_RECEIVABLE_SUMMARY" type="formattext"/>
      <exec message="HIDE" target="_RECEIVABLE_DETAILS" type="formattext"/>
      <exec type="guard">
         <conditions>
            <condition lhsfield="ID" operator="=" rhsvalue="1"/>
         </conditions>
         <exec message="Receivable Summary" target="DESCRIPTION" type="formattext"/>
         <exec message="RECEIVABLE_SUMMARY" target="CODE" type="formattext"/>
         <exec message="SHOW" target="_RECEIVABLE_SUMMARY" type="formattext"/>
      </exec>
      <exec type="guard">
         <conditions>
            <condition lhsfield="ID" operator="=" rhsvalue="2"/>
         </conditions>
         <exec message="Receivable details" target="DESCRIPTION" type="formattext"/>
         <exec message="RECEIVABLE_DETAILS" target="CODE" type="formattext"/>
         <exec message="SHOW" target="_RECEIVABLE_DETAILS" type="formattext"/>
      </exec>
      <exec type="guard">
         <conditions>
            <condition lhsfield="ID" operator="=" rhsvalue="3"/>
         </conditions>
         <exec message="Payments Summary" target="DESCRIPTION" type="formattext"/>
         <exec message="PAYMENT_SUMMARY" target="CODE" type="formattext"/>
         <exec message="SHOW" target="_PAYMENT_SUMMARY" type="formattext"/>
      </exec>
      <exec type="guard">
         <conditions>
            <condition lhsfield="ID" operator="=" rhsvalue="4"/>
         </conditions>
         <exec message="Payments Reports" target="DESCRIPTION" type="formattext"/>
         <exec message="PAYMENT_REPORTS" target="CODE" type="formattext"/>
         <exec message="SHOW" target="_PAYMENT_REPORTS" type="formattext"/>
      </exec>
      <exec type="guard">
         <conditions>
            <condition lhsfield="ID" operator="=" rhsvalue="5"/>
         </conditions>
         <exec message="Attempts" target="DESCRIPTION" type="formattext"/>
         <exec message="_ATTEMPTS" target="CODE" type="formattext"/>
         <exec message="SHOW" target="_ATTEMPTS" type="formattext"/>
      </exec>
   </exec>
   <exec argument="ACCOUNT_FILTERS" index="ID" type="loop">
      <exec type="guard">
         <conditions>
            <condition lhsfield="ID" operator="=" rhsvalue="1"/>
         </conditions>
         <exec message="All" target="DESCRIPTION" type="formattext"/>
         <exec message="ALL" target="CODE" type="formattext"/>
      </exec>
      <exec type="guard">
         <conditions>
            <condition lhsfield="ID" operator="=" rhsvalue="2"/>
         </conditions>
         <exec message="Non Charge Off" target="DESCRIPTION" type="formattext"/>
         <exec message="NCO" target="CODE" type="formattext"/>
      </exec>
      <exec type="guard">
         <conditions>
            <condition lhsfield="ID" operator="=" rhsvalue="3"/>
         </conditions>
         <exec message="Charge Off" target="DESCRIPTION" type="formattext"/>
         <exec message="CO" target="CODE" type="formattext"/>
      </exec>
      <exec type="guard">
         <conditions>
            <condition lhsfield="ID" operator="=" rhsvalue="4"/>
         </conditions>
         <exec message="Pre NPF" target="DESCRIPTION" type="formattext"/>
         <exec message="PRE_NPF" target="CODE" type="formattext"/>
      </exec>
      <exec type="guard">
         <conditions>
            <condition lhsfield="ID" operator="=" rhsvalue="5"/>
         </conditions>
         <exec message="NPF" target="DESCRIPTION" type="formattext"/>
         <exec message="NPF" target="CODE" type="formattext"/>
      </exec>
   </exec>
   <exec argument="PERIODS" index="ID" type="loop">
      <exec type="guard">
         <conditions>
            <condition lhsfield="ID" operator="=" rhsvalue="1"/>
         </conditions>
         <exec message="Effective Date" target="DESCRIPTION" type="formattext"/>
         <exec message="EFFECTIVE_DATE" target="CODE" type="formattext"/>
      </exec>
      <exec type="guard">
         <conditions>
            <condition lhsfield="ID" operator="=" rhsvalue="2"/>
         </conditions>
         <exec message="Acknowledgement Date" target="DESCRIPTION" type="formattext"/>
         <exec message="ACKNOWLEDGEMENT_DATE" target="CODE" type="formattext"/>
      </exec>
      <exec type="guard">
         <conditions>
            <condition lhsfield="ID" operator="=" rhsvalue="3"/>
         </conditions>
         <exec message="Assignment Date" target="DESCRIPTION" type="formattext"/>
         <exec message="ASSIGNMENT_DATE" target="CODE" type="formattext"/>
      </exec>
   </exec>
   <exec from="ACCOUNT_FILTERS" key="ACCOUNT_FILTERS" type="copyintolist"/>
   <exec from="PRODUCT_CATEGORIES" key="PRODUCT_CATEGORIES" type="copyintolist"/>
   <exec from="CATEGORIES" key="CATEGORIES" type="copyintolist"/>
   <exec from="PERIODS" key="PERIODS" type="copyintolist"/>
   <exec from="TREE_LIST" key="TREE_LIST" type="copyintolist"/>
</event>