<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec count="1" type="createlist"/>
 <exec from="minimize" key="minimize" type="copyintolist"/>
 <exec from="maximize" key="maximize" type="copyintolist"/>
 <exec type="loop">
  <exec type="guard">
   <conditions>
    <condition lhsfield="minimize" operator="=" rhsvalue="true"/>
   </conditions>
   <exec message="SHOW" target="showMaximizer" type="formattext"/>
   <exec message="HIDE" target="showMinimizer" type="formattext"/>
   <exec message="HIDE" target="showDetails" type="formattext"/>
   <exec from="minimize" to="clearMinimizer" type="copy"/>
   <exec message="Click maximize button to maximize account information" target="minimizerMessage" type="formattext"/>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="maximize" operator="=" rhsvalue="true"/>
   </conditions>
   <exec message="HIDE" target="showMaximizer" type="formattext"/>
   <exec message="SHOW" target="showMinimizer" type="formattext"/>
   <exec message="SHOW" target="showDetails" type="formattext"/>
   <exec from="maximize" to="clearMinimizer" type="copy"/>
   <exec message="Click minimize button to minimize account information" target="minimizerMessage" type="formattext"/>
  </exec>
 </exec>
</event>
