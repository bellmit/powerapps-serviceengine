<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec count="1" type="createlist"/>
 <exec type="parallelquery">
  <query value="getParentInstructions"/>
  <query value="getActionStages"/>
  <query value="getActionBillingStages"/>
  <query value="getActionCategory"/>
  <query value="getHostCCRISStatus"/>
  <target value="PARENT_INSTRUCTIONS"/>
  <target value="STAGES"/>
  <target value="BILLING_STAGES"/>
  <target value="CATEGORIES"/>
  <target value="HOST_CCRIS_STATUS"/>
 </exec>
