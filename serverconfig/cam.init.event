<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec type="guard">
  <conditions>
   <condition lhsfield="ACCOUNT_ID" operator="is not empty" rhsvalue=""/>
   <condition lhsfield="CUSTOMER_ID" operator="is not empty" rhsvalue=""/>
   <condition lhsfield="ACCOUNT_ID" operator="&lt;&gt;" rhsvalue="parent.ACCOUNT_ID"/>
   <condition lhsfield="CUSTOMER_ID" operator="&lt;&gt;" rhsvalue="parent.CUSTOMER_ID"/>
  </conditions>
  <exec type="guard">
   <conditions>
    <condition lhsfield="TRUE" operator="=" rhsvalue=""/>
   </conditions>
   <exec statement="booleanvalues" type="select"/>
  </exec>
  <exec event="cam.usersetting" ignoreresult="true" type="raise"/>
  <exec from="SETTING" type="copy"/>
  <exec from="TRUE" key="RELOAD_PAGE" type="copyintolist"/>
  <exec from="TRUE" key="RELOAD_TAB" type="copyintolist"/>
 </exec>
 <exec type="guard">
  <conditions>
   <condition lhsfield="SETTING" operator="is empty" rhsvalue=""/>
  </conditions>
  <exec count="1" type="createlist"/>
  <exec message="SHOW" target="_LOADING" type="formattext"/>
  <exec from="_LOADING" key="_LOADING" type="copyintolist"/>
 </exec>
</event>
