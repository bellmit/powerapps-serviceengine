<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="true" type="event">
 <exec count="1" type="createlist"/>
 <exec to="LOGON_USER" type="user"/>
 <exec ceiling="false" endmonth="false" nextweekday="false" startmonth="false" truncate="false" type="date"/>
 <exec statement="getOneToFive" type="select"/>
 <exec from="ONE" key="ONE" to="SPREADSHEET_LIST" type="copyintolist"/>
 <exec from="TWO" key="TWO" to="SPREADSHEET_LIST" type="copyintolist"/>
 <exec from="THREE" key="THREE" to="SPREADSHEET_LIST" type="copyintolist"/>
 <exec from="FOUR" key="FOUR" to="SPREADSHEET_LIST" type="copyintolist"/>
 <exec key="VALID" minuend="SPREADSHEET_LIST" subtrahend="FALSE" to="SPREADSHEET_LIST" type="remove"/>
 <exec from="LOGON_USER" key="CREATED_USER_ID" to="SPREADSHEET_LIST" type="copyintolist"/>
 <exec from="CURRENT_DATE" key="CREATED_TIME" to="SPREADSHEET_LIST" type="copyintolist"/>
 <exec type="guard">
  <conditions>
   <condition lhsfield="TYPE_ID" operator="=" rhsvalue="1"/>
  </conditions>
  <exec argument="SPREADSHEET_LIST" type="loop">
   <exec event="ramci.saveContactNumber" ignoreresult="true" type="raisewith"/>
   <exec statement="insertRamciContactNumberHistory" type="insert"/>
  </exec>
 </exec>
 <exec type="guard">
  <conditions>
   <condition lhsfield="TYPE_ID" operator="=" rhsvalue="2"/>
  </conditions>
  <exec argument="SPREADSHEET_LIST" type="loop">
   <exec event="ramci.saveAddress" ignoreresult="true" type="raisewith"/>
   <exec statement="insertRamciAddressHistory" type="insert"/>
  </exec>
 </exec>
 <exec message="Save Successful" target="MESSAGE" type="formattext"/>
 <exec from="TRUE" key="OPEN_MESSAGE" type="copyintolist"/>
 <exec from="MESSAGE" key="MESSAGE" type="copyintolist"/>
</event>
