<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec count="1" type="createlist"/>
 <exec ceiling="false" endmonth="false" nextweekday="false" startmonth="false" truncate="true" type="date"/>
 <exec type="parallelquery">
  <query value="getKpiCurrentUserWorklist"/>
  <target value="CURRENT_WORKLIST_ASSIGNMENT"/>
 </exec>
 <exec from="CURRENT_WORKLIST_ASSIGNMENT" key="WORK_LIST_ID" to="WORKLIST" type="extract"/>
 <exec type="parallelquery">
  <query value="getKpiWorklistSummary"/>
  <query value="getKpiDailyWorklistReceivableSummary"/>
  <target value="MTD_SUMMARY"/>
  <target value="DAILY_SUMMARY"/>
 </exec>
 <exec from="CURRENT_WORKLIST_ASSIGNMENT" to="DAILY_RECEIVABLES" type="copy"/>
 <exec from="CURRENT_WORKLIST_ASSIGNMENT" to="MONTH_TO_DATE_RECEIVABLES" type="copy"/>
 <exec from="DAILY_SUMMARY" key="WORK_LIST_ID" to="DAILY_RECEIVABLES" type="merge"/>
 <exec from="MTD_SUMMARY" key="WORK_LIST_ID" to="MONTH_TO_DATE_RECEIVABLES" type="merge"/>
 <exec from="TRUE" key="VIEW_DAILY_DETAILS" to="DAILY_RECEIVABLES" type="copyintolist"/>
 <exec from="TRUE" key="VIEW_MTD_DETAILS" to="MONTH_TO_DATE_RECEIVABLES" type="copyintolist"/>
 <exec from="DAILY_RECEIVABLES" key="DAILY_RECEIVABLES" type="copyintolist"/>
 <exec from="MONTH_TO_DATE_RECEIVABLES" key="MONTH_TO_DATE_RECEIVABLES" type="copyintolist"/>
</event>
