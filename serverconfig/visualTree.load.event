<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec count="1" type="createlist"/>
 <exec from="HEADER_TITLE" key="TREE_TYPE" type="copyintolist"/>
 <exec type="loop">
  <exec type="guard">
   <conditions>
    <condition lhsfield="TREE_TYPE" operator="=" rhsvalue="Global Settings - Visual Tree - SMS"/>
   </conditions>
   <exec statement="getDicisionTreeTypeSms" type="select"/>
   <exec from="TREE_TYPE_ID" to="TREE_TYPE_ID" type="copy"/>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="TREE_TYPE" operator="=" rhsvalue="Global Settings - Visual Tree - Letter"/>
   </conditions>
   <exec statement="getDicisionTreeTypeLetter" type="select"/>
   <exec from="TREE_TYPE_ID" to="TREE_TYPE_ID" type="copy"/>
  </exec>
  <exec argument="EVALUATION_TREE_LIST" query="getDecisionTreeByType" type="query"/>
  <exec argument="TREE_CONDITIONS" query="getDecisionTreeCondition" type="query"/>
 </exec>
</event>
