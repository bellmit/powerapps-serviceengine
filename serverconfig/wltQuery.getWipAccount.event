<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec ceiling="false" endmonth="false" nextweekday="false" startmonth="true" to="FIRST_OF_MONTH" truncate="true" type="date"/>
 <exec ceiling="false" endmonth="false" nextweekday="false" startmonth="false" truncate="false" type="date"/>
 <exec ceiling="false" endmonth="true" nextweekday="false" startmonth="false" to="END_OF_MONTH" truncate="true" type="date"/>
 <exec argument="EXCLUDEDACCOUNTS" query="selectEmptyList" type="query"/>
 <exec argument="WIP_CALL_TYPE_LIST" query="getWltCallWipTypeStatuses" type="query"/>
 <exec text="Treatments In progress Accounts" type="comment">
  <exec text="Fetch WLT Evaluation In Progress Accounts" type="comment">
   <exec argument="WIP_ACCOUNT_LIST" query="getInProgressAccountData" type="query"/>
  </exec>
 </exec>
 <exec text="Unimplemented" type="comment">
  <exec type="guard">
   <conditions>
    <condition lhsfield="notImplemented" operator="=" rhsvalue="_xx_"/>
   </conditions>
   <exec argument="REPO_ACCOUNT_LIST" query="getInProgressAccountDataForRepo" type="query"/>
   <exec argument="SHORTFALL_ACCOUNT_LIST" query="getInProgressAccountDataForShortfall" type="query"/>
   <exec text="SI Accounts" type="comment">
    <exec text="Fetch All SI Accounts" type="comment">
     <exec argument="SI_ACCOUNT_LIST" query="getSIAccountIdforWorklist" type="query"/>
     <exec from="SI_ACCOUNT_LIST" key="ACCOUNT_ID" to="SI_ACCOUNT_ID_LIST" type="extract"/>
     <exec from="SI_ACCOUNT_ID_LIST" to="SI_ACCOUNTS" type="copy"/>
     <exec from="SI_ACCOUNT_ID_LIST" to="IN_PROGRESS_ACCOUNT_LIST" type="append"/>
    </exec>
    <exec text="Fetch Expired SI Accounts" type="comment">
     <exec argument="EXPIRED_SI_ACCOUNT_LIST" query="getExpiredSIAccountIdforWorklist" type="query"/>
     <exec from="EXPIRED_SI_ACCOUNT_LIST" key="ACCOUNT_ID" to="SI_EXPIRED_ACCOUNTS" type="extract"/>
    </exec>
   </exec>
   <exec text="PDC Accounts" type="comment">
    <exec text="Fetch PDC Accounts" type="comment">
     <exec argument="PDC_ACCOUNT_LIST" query="getPdcWorkInProgressAccount" type="query"/>
     <exec from="PDC_ACCOUNT_LIST" key="ACCOUNT_ID" to="PDC_ACCOUNT_ID_LIST" type="extract"/>
     <exec from="PDC_ACCOUNT_ID_LIST" to="PDC_ACCOUNTS" type="copy"/>
     <exec from="PDC_ACCOUNT_ID_LIST" to="IN_PROGRESS_ACCOUNT_LIST" type="append"/>
    </exec>
    <exec text="Fetch Bounced Payment Accounts" type="comment">
     <exec argument="BOUNCE_ACCOUNT_LIST" query="getPdcBouncedPaymentsWorkInProgressAccount" type="query"/>
     <exec from="BOUNCE_ACCOUNT_LIST" key="ACCOUNT_ID" to="BOUNCE_PYMT_ACCOUNTS" type="extract"/>
    </exec>
    <exec text="Fetch Expired PDC Accounts" type="comment">
     <exec argument="EXPIRED_PDC_ACCOUNT_LIST" query="getPdcExpiredWorkInProgressAccount" type="query"/>
     <exec from="EXPIRED_PDC_ACCOUNT_LIST" key="ACCOUNT_ID" to="PDC_EXPIRED_ACCOUNTS" type="extract"/>
    </exec>
   </exec>
   <exec text="OCA Accounts" type="comment">
    <exec text="Fetch OCA Accounts" type="comment">
     <exec argument="OCA_ACCOUNT_LIST" query="getOCAAccountsForWorkList" type="query"/>
     <exec from="OCA_ACCOUNT_LIST" key="ACCOUNT_ID" to="OCA_ACCOUNT_ID_LIST" type="extract"/>
     <exec from="OCA_ACCOUNT_ID_LIST" to="OCA_ACCOUNTS" type="copy"/>
     <exec from="OCA_ACCOUNT_ID_LIST" to="IN_PROGRESS_ACCOUNT_LIST" type="append"/>
    </exec>
   </exec>
  </exec>
 </exec>
 <exec text="Collate WIP list" type="comment">
  <exec from="REPO_ACCOUNT_LIST" to="WIP_ACCOUNT_LIST" type="append"/>
  <exec from="SHORTFALL_ACCOUNT_LIST" to="WIP_ACCOUNT_LIST" type="append"/>
 </exec>
</event>
