<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec type="guard">
  <conditions>
   <condition lhsfield="TYPE_ID" operator="=" rhsvalue="1"/>
  </conditions>
  <exec message="0" target="MINUTES" type="formattext"/>
  <exec type="guard">
   <conditions>
    <condition lhsfield="SELECTED_MINUTES" operator="is not empty" rhsvalue=""/>
   </conditions>
   <exec list="SELECTED_MINUTES" to="SELECTED_MINUTES_COUNT" type="countlist"/>
   <exec type="guard">
    <conditions>
     <condition lhsfield="SELECTED_MINUTES_COUNT" operator="&lt;&gt;" rhsvalue="0"/>
    </conditions>
    <exec from="SELECTED_MINUTES" key="ID" to="MINUTES_CRON" type="extract"/>
    <exec message="{0}" target="MINUTES_CRON" type="formattext">
     <key value="MINUTES_CRON"/>
    </exec>
    <exec from="MINUTES_CRON" regex="\s" replacement="" to="MINUTES" type="regexreplace"/>
   </exec>
  </exec>
  <exec message="*" target="HOUR" type="formattext"/>
