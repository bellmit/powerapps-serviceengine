<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec argument="DATA_LIST" query="getLegalAbortEvaluationTree" type="query"/>
 <exec argument="DATA_LIST" type="loop">
  <exec type="guard">
   <conditions>
    <condition lhsfield="IS_BATCH" operator="=" rhsvalue="true"/>
   </conditions>
   <exec message="&lt;html&gt;&lt;p style=&quot;color:#0094EE;&quot;&gt;{0}&lt;/p&gt;&lt;/html&gt;" target="DESCRIPTION" type="formattext">
    <key value="DESCRIPTION"/>
   </exec>
  </exec>
 </exec>
 <exec count="1" type="createlist"/>
 <exec type="guard">
  <conditions>
   <condition lhsfield="SHOW_ALL" operator="=" rhsvalue="true"/>
  </conditions>
  <exec from="FALSE" key="DISABLE" to="DATA_LIST" type="copyintolist"/>
 </exec>
 <exec type="guard">
  <conditions>
   <condition lhsfield="DATA_LIST" operator="is empty" rhsvalue=""/>
  </conditions>
  <exec count="1" to="DATA_LIST" type="createlist"/>
  <exec message="No Abort Stategy yet " target="DESCRIPTION" type="formattext"/>
  <exec from="DESCRIPTION" key="DESCRIPTION" to="DATA_LIST" type="copyintolist"/>
 </exec>
 <exec from="DATA_LIST" key="DATA_LIST" type="copyintolist"/>
 <exec from="SELECTED" key="SELECTED" type="copyintolist"/>
</event>
