   </conditions>
   <exec list="SELECTED_MONTHS" to="SELECTED_MONTHS_COUNT" type="countlist"/>
   <exec type="guard">
    <conditions>
     <condition lhsfield="SELECTED_MONTHS_COUNT" operator="&lt;&gt;" rhsvalue="0"/>
