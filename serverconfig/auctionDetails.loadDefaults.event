<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec count="1" to="DEFAULT_LIST" type="createlist"/>
 <exec from="ACCOUNT_ID" key="ACCOUNT_ID" to="DEFAULT_LIST" type="copyintolist"/>
 <exec from="PROCESS_TYPE_ID" key="PROCESS_TYPE_ID" to="DEFAULT_LIST" type="copyintolist"/>
 <exec type="parallelquery">
  <query value="getDefaultTreatmentStatusType"/>
  <target value="TYPE_STATUS_LIST"/>
 </exec>
 <exec statement="getTreatmentAuctionDetails" type="select"/>
 <exec type="guard">
  <conditions>
   <condition lhsfield="1" operator="=" rhsvalue="2"/>
  </conditions>
  <exec argument="SHORTFALL_LIST" query="getShortfallNoticeReadOnlyFields" type="query"/>
 </exec>
 <exec type="guard">
  <conditions>
   <condition lhsfield="1" operator="=" rhsvalue="2"/>
  </conditions>
  <exec type="parallelquery">
   <query value="getAuctionDetailsAuctionFee"/>
   <query value="getAuctionDetailsStorageFee"/>
   <query value="getAuctionDetailsRepossessionFee"/>
   <query value="getAuctionDetailsOtherIncidentalCostPaid"/>
   <target value="AUCTION_FEE_LIST"/>
   <target value="STORAGE_FEE_LIST"/>
   <target value="REPOSSESSION_FEE_LIST"/>
   <target value="OTHER_INCIDENTAL_LIST"/>
  </exec>
 </exec>
 <exec from="SHORTFALL_LIST" key="ACCOUNT_ID" to="DEFAULT_LIST" type="merge"/>
 <exec from="AUCTION_FEE_LIST" key="ACCOUNT_ID" to="DEFAULT_LIST" type="merge"/>
 <exec from="REPOSSESSION_FEE_LIST" key="ACCOUNT_ID" to="DEFAULT_LIST" type="merge"/>
 <exec from="STORAGE_FEE_LIST" key="ACCOUNT_ID" to="DEFAULT_LIST" type="merge"/>
 <exec from="OTHER_INCIDENTAL_LIST" key="ACCOUNT_ID" to="DEFAULT_LIST" type="merge"/>
 <exec from="TYPE_STATUS_LIST" key="TYPE_STATUS_LIST" to="DEFAULT_LIST" type="copyintolist"/>
 <exec from="COLLATERAL_CODE" key="COLLATERAL_CODE" to="DEFAULT_LIST" type="copyintolist"/>
 <exec from="COLLATERAL_NAME" key="COLLATERAL_NAME" to="DEFAULT_LIST" type="copyintolist"/>
 <exec from="DATE_OF_REGISTRATION" key="DATE_OF_REGISTRATION" to="DEFAULT_LIST" type="copyintolist"/>
 <exec from="YEAR_OF_MANUFACTURE" key="YEAR_OF_MANUFACTURE" to="DEFAULT_LIST" type="copyintolist"/>
 <exec from="DEFAULT_LIST" type="copy"/>
 <exec type="guard">
  <conditions>
   <condition lhsfield="1" operator="=" rhsvalue="2"/>
  </conditions>
  <exec argument="DEFAULT_LIST" type="loop">
   <exec statement="getTreatmentAuctionCollateralDetails" type="select"/>
   <exec statement="getStoreYardforAuction" type="select"/>
   <exec statement="get14DaysDetailsforAuction" type="select"/>
   <exec statement="getAuctionDetailsFifthScheduleDate" type="select"/>
   <exec statement="getShortfallDate" type="select"/>
   <exec statement="getAccountFSDate" type="select"/>
   <exec statement="getAccountNIDate" type="select"/>
   <exec argument="AUCTION_STATUS_LIST" query="getAuctionHistory" type="query"/>
   <exec message="Please Select" target="PLEASE_SELECT" type="formattext"/>
   <exec text="For Technical Reason Description List" type="comment">
    <exec argument="ALL_TECH_REASON_DESCRIPTION_LIST" query="getTechnicalReasonDescriptionForAuctionDetails" type="query"/>
    <exec count="1" to="TECH_REASON_DESCRIPTION_LIST" type="createlist"/>
    <exec from="PLEASE_SELECT" key="DESCRIPTION" to="TECH_REASON_DESCRIPTION_LIST" type="copyintolist"/>
    <exec from="ALL_TECH_REASON_DESCRIPTION_LIST" to="TECH_REASON_DESCRIPTION_LIST" type="append"/>
   </exec>
   <exec text="For Auction Date List" type="comment">
    <exec argument="ALL_AUCTION_DATE_LIST" query="get14DaysDateforAucaytion" type="query"/>
    <exec count="1" to="AUCTION_DATE_LIST" type="createlist"/>
    <exec from="PLEASE_SELECT" key="AUCTION_DATE_AUCTION" to="AUCTION_DATE_LIST" type="copyintolist"/>
    <exec from="ALL_AUCTION_DATE_LIST" to="AUCTION_DATE_LIST" type="append"/>
    <exec argument="AUCTION_DATE_LIST" index="ID" type="loop"/>
   </exec>
   <exec text="For Auction Date List" type="comment">
    <exec argument="ALL_SOLD_DATE_DATE_LIST" query="get14DaysDateforAucaytionDateSold" type="query"/>
    <exec count="1" to="SOLD_DATE_LIST" type="createlist"/>
    <exec from="PLEASE_SELECT" key="SOLD_DATE_AUCTION" to="SOLD_DATE_LIST" type="copyintolist"/>
    <exec from="ALL_SOLD_DATE_DATE_LIST" to="SOLD_DATE_LIST" type="append"/>
   </exec>
  </exec>
 </exec>
</event>
