<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec count="1" type="createlist"/>
 <exec ceiling="false" endmonth="false" nextweekday="false" startmonth="false" to="CURRENT_TIME" truncate="false" type="date"/>
 <exec type="parallelquery">
  <query value="getScheduledReportsNextRunTime"/>
  <query value="getScheduledReports"/>
  <target value="NEXT_RUNTIME"/>
  <target value="SCHEDULED_REPORTS"/>
 </exec>
 <exec from="NEXT_RUNTIME" key="REPORT_ID" to="SCHEDULED_REPORTS" type="merge"/>
 <exec argument="SCHEDULED_REPORTS" type="loop">
