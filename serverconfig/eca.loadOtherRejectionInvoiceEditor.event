<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec count="1" type="createlist"/>
 <exec type="guard">
  <conditions>
   <condition lhsfield="INVOICE_ID" operator="is not empty" rhsvalue=""/>
   <condition lhsfield="INVOICE_ID" operator="&lt;&gt;" rhsvalue="parent.INVOICE_ID"/>
   <condition lhsfield="TRANSACTION_ID" operator="is not empty" rhsvalue=""/>
   <condition lhsfield="TRANSACTION_ID" operator="&lt;&gt;" rhsvalue="parent.TRANSACTION_ID"/>
  </conditions>
  <exec statement="getOtherRejectionInvoicesTransaction" type="select"/>
 </exec>
 <exec from="OTHER_REJECTION" key="OTHER_REJECTION" type="copyintolist"/>
</event>
