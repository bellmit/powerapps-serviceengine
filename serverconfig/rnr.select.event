  <conditions>
   <condition lhsfield="PRODUCT_CATEGORY" operator="=" rhsvalue="PFI"/>
  </conditions>
  <exec from="VARIABLE_SETTLEMENT_LIST" to="HP_VARIABLE_SETTLEMENT_LIST" type="copy"/>
