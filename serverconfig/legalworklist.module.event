<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec count="1" type="createlist"/>
 <exec count="2" to="MODULES" type="createlist"/>
 <exec from="TRUE" key="DISABLE" to="MODULES" type="copyintolist"/>
 <exec from="TRUE" key="TRUE" to="MODULES" type="copyintolist"/>
 <exec from="FALSE" key="FALSE" to="MODULES" type="copyintolist"/>
 <exec from="TRUE" key="LOAD_PAGE" to="MODULES" type="copyintolist"/>
 <exec from="DEFAULT_TITLE" key="DEFAULT_TITLE" to="MODULES" type="copyintolist"/>
 <exec to="LOGON_USER" type="user"/>
 <exec statement="getLoginUserType" type="select"/>
 <exec from="USER_TYPE_ID" key="USER_TYPE_ID" to="MODULES" type="copyintolist"/>
 <exec argument="MODULES" index="ID" type="loop">
  <exec message="HIDE" target="_PAGE_1" type="formattext"/>
  <exec message="HIDE" target="_PAGE_2" type="formattext"/>
  <exec type="guard">
   <conditions>
    <condition lhsfield="ID" operator="=" rhsvalue="1"/>
   </conditions>
   <exec message="Worklist Manager" target="DESCRIPTION" type="formattext"/>
   <exec message="WORLIST" target="CODE" type="formattext"/>
   <exec message="SHOW" target="_PAGE_1" type="formattext"/>
   <exec type="guard">
    <conditions>
     <condition lhsfield="USER_TYPE_ID" operator="=" rhsvalue="1"/>
    </conditions>
    <exec from="920000" type="authorized">
     <exec from="FALSE" to="DISABLE" type="copy"/>
    </exec>
   </exec>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="ID" operator="=" rhsvalue="2"/>
   </conditions>
   <exec message="VISUAL" target="CODE" type="formattext"/>
   <exec message="SHOW" target="_PAGE_2" type="formattext"/>
   <exec message="Visual Worklist Assignment" target="DESCRIPTION" type="formattext"/>
   <exec type="guard">
    <conditions>
     <condition lhsfield="USER_TYPE_ID" operator="=" rhsvalue="1"/>
    </conditions>
    <exec from="920001" type="authorized">
     <exec from="FALSE" to="DISABLE" type="copy"/>
    </exec>
   </exec>
  </exec>
  <exec message="{0} - {1}" target="HEADER_TITLE" type="formattext">
   <key value="DEFAULT_TITLE"/>
   <key value="DESCRIPTION"/>
  </exec>
 </exec>
 <exec from="MODULES" key="MODULES" type="copyintolist"/>
</event>
