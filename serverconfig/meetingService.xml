<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="meetingService">







	<insert id="insertTreatmentMeeting">
		insert into PTRTREATMENT_MEETING
		(TREATMENT_ID,ACCOUNT_ID,TYPE_STATUS_ID,SUBTYPE_ID,MEETING_TIME,RELATIONSHIP_TYPE,LOCATION,PERSON_TO_MEET)
		values
		(#ID#, #ACCOUNT_ID#,#TYPE_STATUS_ID#,#SUBTYPE_ID#,#MEETING_TIME#,#RELATIONSHIP_TYPE#,#LOCATION#,#PERSON_TO_MEET#)
	</insert>
	<update id="updateTreatmentMeeting">
	update PTRTREATMENT_MEETING set TYPE_STATUS_ID=#TYPE_STATUS_ID#,
	SUBTYPE_ID = #SUBTYPE_ID#,MEETING_TIME=#MEETING_TIME#,RELATIONSHIP_TYPE=#RELATIONSHIP_TYPE#,
	LOCATION=#LOCATION#,PERSON_TO_MEET=#PERSON_TO_MEET# where TREATMENT_ID=#ID#
	</update>
	<resultMap id="getTreatmentMeeting" class="hmap">
		<result property="TYPE_STATUS_ID" javaType="long" />
		<result property="SUBTYPE_ID" javaType="long" />
		<result property="MEETING_TIME" javaType="ts" />
		<result property="RELATIONSHIP_TYPE" javaType="string" />
		<result property="LOCATION" javaType="string" />
		<result property="PERSON_TO_MEET" javaType="string" />
	</resultMap>	
	
	<select id="getTreatmentMeeting" resultMap="getTreatmentMeeting">		
		SELECT TYPE_STATUS_ID,SUBTYPE_ID,MEETING_TIME,RELATIONSHIP_TYPE,LOCATION,PERSON_TO_MEET FROM PTRTREATMENT_MEETING
		WHERE TREATMENT_ID=#ID#
	</select>	
</sqlMap>