<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec statement="getOneToFive" type="select"/>
 <exec statement="selectNegativeOne" type="select"/>
 <exec statement="getTrue" type="select"/>
 <exec type="guard">
  <conditions>
   <condition lhsfield="NUMBER_OF_ACCOUNTS" operator="=" rhsvalue="0"/>
  </conditions>
  <exec from="TRUE" to="PREVIOUS_DISABLE" type="copy"/>
  <exec from="TRUE" to="NEXT_DISABLE" type="copy"/>
  <exec from="ZERO" to="PAGE_NUMBER" type="copy"/>
  <exec from="ZERO" to="DIVIDED_NUMBER" type="copy"/>
 </exec>
 <exec type="guard">
  <conditions>
   <condition lhsfield="PAGE_NUMBER" operator="is empty" rhsvalue=""/>
  </conditions>
  <exec from="ZERO" to="PAGE_NUMBER" type="copy"/>
  <exec from="TRUE" to="NEXT" type="copy"/>
 </exec>
 <exec type="guard">
  <conditions>
   <condition lhsfield="NUMBER_OF_ACCOUNTS" operator="&lt;&gt;" rhsvalue="0"/>
  </conditions>
  <exec lhs="NUMBER_OF_ACCOUNTS" rhs="LIMIT_RESULT" target="DIVIDED_NUMBER" type="divide"/>
  <exec from="DIVIDED_NUMBER" to="NUMBER_OF_PAGES" type="copy"/>
  <exec lhs="LIMIT_RESULT" rhs="NUMBER_OF_PAGES" target="TOTAL_ACCOUNTS" type="multiply"/>
  <exec lhs="NEGATIVE_ONE" rhs="TOTAL_ACCOUNTS" target="NEGATIVE_TOTAL_ACCOUNTS" type="multiply"/>
  <exec lhs="NUMBER_OF_ACCOUNTS" rhs="NEGATIVE_TOTAL_ACCOUNTS" target="REMAINING_ACCOUNTS" type="add"/>
  <exec type="guard">
   <conditions>
    <condition lhsfield="REMAINING_ACCOUNTS" operator="greater than value of" rhsvalue="ZERO"/>
   </conditions>
   <exec lhs="DIVIDED_NUMBER" rhs="ONE" target="DIVIDED_NUMBER" type="add"/>
  </exec>
 </exec>
 <exec type="guard">
  <conditions>
   <condition lhsfield="DIVIDED_NUMBER" operator="=" rhsvalue="1"/>
  </conditions>
  <exec from="TRUE" to="NEXT_DISABLE" type="copy"/>
 </exec>
 <exec type="guard">
  <conditions>
   <condition lhsfield="PREVIOUS" operator="exists" rhsvalue=""/>
  </conditions>
  <exec lhs="PAGE_NUMBER" rhs="NEGATIVE_ONE" target="PAGE_NUMBER" type="add"/>
  <exec from="FALSE" to="NEXT_DISABLE" type="copy"/>
 </exec>
 <exec type="guard">
  <conditions>
   <condition lhsfield="NEXT" operator="exists" rhsvalue=""/>
  </conditions>
  <exec lhs="PAGE_NUMBER" rhs="ONE" target="PAGE_NUMBER" type="add"/>
 </exec>
 <exec type="guard">
  <conditions>
   <condition lhsfield="PAGE_NUMBER" operator="=" rhsvalue="1"/>
  </conditions>
  <exec from="TRUE" to="PREVIOUS_DISABLE" type="copy"/>
 </exec>
 <exec text="Figure out if Next should be disabled, are we on the last page?" type="comment">
  <exec lhs="PAGE_NUMBER" rhs="NEGATIVE_ONE" target="NEGATIVE_PAGE_NUMBER" type="multiply"/>
  <exec lhs="DIVIDED_NUMBER" rhs="NEGATIVE_PAGE_NUMBER" target="PAGES_REMAINING" type="add"/>
  <exec type="guard">
   <conditions>
    <condition lhsfield="PAGES_REMAINING" operator="less than value of" rhsvalue="ONE"/>
   </conditions>
   <exec from="TRUE" to="NEXT_DISABLE" type="copy"/>
  </exec>
 </exec>
 <exec message="{0} / {1}" target="PAGE_NO" type="formattext">
  <key value="PAGE_NUMBER"/>
  <key value="DIVIDED_NUMBER"/>
 </exec>
</event>
