<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec type="guard">
  <conditions>
   <condition lhsfield="PROCESS_TYPE_ID" operator="is not empty" rhsvalue=""/>
  </conditions>
  <exec count="1" type="createlist"/>
  <exec ceiling="false" endmonth="false" nextweekday="false" startmonth="false" to="FILE_REVIEW_DATE" truncate="true" type="date"/>
  <exec statement="selectNegativeOne" type="select"/>
  <exec type="parallelquery">
   <query value="getDefaultTreatmentStatusType"/>
   <query value="getDefaultTreatmentSubType"/>
   <query value="getYesNoPleaseSelect"/>
   <query value="getAccountReviewStatus"/>
   <query value="getAccountReviewRecoverableStatus"/>
   <target value="TYPE_STATUS_LIST"/>
   <target value="SUBTYPE_LIST"/>
   <target value="YES_NO_LIST"/>
   <target value="REVIEW_STATUS_LIST"/>
   <target value="RECOVERABLE_STATUS_LIST"/>
  </exec>
  <exec type="parallelquery">
   <query value="getAccountReviewDefaultReason"/>
   <query value="getAccountReviewCustomers"/>
   <target value="REASON_OF_DEFAULT_LIST"/>
   <target value="CUSTOMER_LIST"/>
  </exec>
  <exec from="NEGATIVE_ONE" key="NEGATIVE_ONE" to="CUSTOMER_LIST" type="copyintolist"/>
  <exec argument="CUSTOMER_LIST" index="SELECTED" type="loop">
   <exec lhs="SELECTED" rhs="NEGATIVE_ONE" target="SELECTED" type="add"/>
  </exec>
  <exec key="IS_DEFAULT" minuend="CUSTOMER_LIST" subtrahend="FALSE" to="SELECTED_LIST" type="remove"/>
  <exec from="SELECTED_LIST" key="CUSTOMER_NAME" to="CUSTOMER_NAME" type="extract"/>
  <exec from="SELECTED_LIST" key="CUSTOMER_ID" to="SELECTED_CUSTOMER_ID" type="extract"/>
  <exec from="SELECTED_LIST" key="CUSTOMER_NAME" to="CUSTOMER_NAME" type="extract"/>
  <exec from="SELECTED_LIST" key="RELATIONSHIP" to="RELATIONSHIP" type="extract"/>
  <exec message="{0}" target="CUSTOMER_NAME" type="formattext">
   <key value="CUSTOMER_NAME"/>
  </exec>
  <exec message="{0}" target="SELECTED_CUSTOMER_ID" type="formattext">
   <key value="SELECTED_CUSTOMER_ID"/>
  </exec>
  <exec message="{0}" target="RELATIONSHIP" type="formattext">
   <key value="RELATIONSHIP"/>
  </exec>
  <exec key="SELECTED" list="SELECTED_LIST" to="SELECTED" type="sumlist"/>
  <exec from="TYPE_STATUS_LIST" key="TYPE_STATUS_LIST" type="copyintolist"/>
  <exec from="SUBTYPE_LIST" key="SUBTYPE_LIST" type="copyintolist"/>
  <exec from="YES_NO_LIST" key="YES_NO_LIST" type="copyintolist"/>
  <exec from="RECOVERABLE_STATUS_LIST" key="RECOVERABLE_STATUS_LIST" type="copyintolist"/>
  <exec from="REVIEW_STATUS_LIST" key="REVIEW_STATUS_LIST" type="copyintolist"/>
  <exec from="REASON_OF_DEFAULT_LIST" key="REASON_OF_DEFAULT_LIST" type="copyintolist"/>
  <exec from="CUSTOMER_LIST" key="CUSTOMER_LIST" type="copyintolist"/>
  <exec from="SELECTED" key="SELECTED" type="copyintolist"/>
  <exec from="FILE_REVIEW_DATE" key="FILE_REVIEW_DATE" type="copyintolist"/>
  <exec from="CUSTOMER_NAME" key="CUSTOMER_NAME" type="copyintolist"/>
  <exec from="SELECTED_CUSTOMER_ID" key="SELECTED_CUSTOMER_ID" type="copyintolist"/>
  <exec from="RELATIONSHIP" key="RELATIONSHIP" type="copyintolist"/>
 </exec>
</event>
