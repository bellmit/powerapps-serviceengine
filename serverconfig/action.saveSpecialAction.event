<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="true" type="event">
 <exec from="STATUS_ID" key="STATUS_ID" to="ACCOUNT_LIST" type="copyintolist"/>
 <exec from="REMARKS" key="REMARKS" to="ACCOUNT_LIST" type="copyintolist"/>
 <exec from="ADDITIONAL_DETAILS" key="ADDITIONAL_DETAILS" to="ACCOUNT_LIST" type="copyintolist"/>
 <exec to="LOGON_USER" type="user"/>
 <exec ceiling="false" endmonth="false" nextweekday="false" startmonth="false" to="CURRENT_TIME" truncate="false" type="date"/>
 <exec from="CURRENT_TIME" key="CURRENT_TIME" to="ACCOUNT_LIST" type="copyintolist"/>
 <exec from="LOGON_USER" key="LOGON_USER" to="ACCOUNT_LIST" type="copyintolist"/>
 <exec from="ACTION_ID" key="ACTION_ID" to="ACCOUNT_LIST" type="copyintolist"/>
 <exec argument="ACCOUNT_LIST" type="loop">
  <exec type="guard">
   <conditions>
    <condition lhsfield="PROCESS_ID" operator="is not empty" rhsvalue=""/>
   </conditions>
   <exec recordcount="" statement="updateSpecialAction" type="update"/>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="PROCESS_ID" operator="is empty" rhsvalue=""/>
   </conditions>
   <exec statement="insertSpecialAction" type="insert"/>
   <exec from="ID" to="PROCESS_ID" type="copy"/>
   <exec recordcount="" statement="updateAssignmentLastAction" type="update"/>
   <exec statement="monitorSpecialAction" type="insert"/>
   <exec event="action.lastLegalActionUpdate" ignoreresult="true" type="raise"/>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="REMARKS" operator="is not empty" rhsvalue=""/>
   </conditions>
   <exec statement="insertLegalActionRemarks" type="insert"/>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="STATUS_ID" operator="&lt;&gt;" rhsvalue="15007"/>
   </conditions>
   <exec statement="clearSpecialAction" type="delete"/>
  </exec>
 </exec>
 <exec count="1" type="createlist"/>
 <exec message="Done!!!" target="MESSAGE" type="formattext"/>
 <exec from="MESSAGE" key="MESSAGE" type="copyintolist"/>
 <exec from="TRUE" key="OPEN_MESSAGE" type="copyintolist"/>
 <exec from="TRUE" key="CLOSE" type="copyintolist"/>
</event>
