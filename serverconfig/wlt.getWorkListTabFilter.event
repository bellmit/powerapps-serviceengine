<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec to="USER_ID" type="user"/>
 <exec statement="getUserUserType" type="select"/>
 <exec type="guard">
  <conditions>
   <condition lhsfield="MODULE_NAME" operator="=" rhsvalue="UPL"/>
  </conditions>
  <exec type="guard">
   <conditions>
    <condition lhsfield="TYPE_CODE" operator="=" rhsvalue="bankuser"/>
   </conditions>
   <exec argument="UPL_WLT_LIST" query="getWorkListTabsReferenceUpl" type="query"/>
  </exec>
  <exec from="UPL_WLT_LIST" to="RESULT_LIST" type="copy"/>
 </exec>
 <exec type="guard">
