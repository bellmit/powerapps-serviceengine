<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="true" type="event">
 <exec type="guard">
  <conditions>
   <condition lhsfield="ADDRESS_TYPE_1" operator="is not empty" rhsvalue=""/>
  </conditions>
  <exec from="ADDRESS_LINE_1_1" to="LINE_1" type="copy"/>
  <exec from="ADDRESS_LINE_2_1" to="LINE_2" type="copy"/>
  <exec from="ADDRESS_LINE_3_1" to="LINE_3" type="copy"/>
