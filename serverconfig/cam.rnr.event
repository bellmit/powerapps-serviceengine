<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec text="New" type="comment">
  <exec count="1" type="createlist"/>
  <exec from="CUSTOMER_ID" key="CUSTOMER_ID" type="copyintolist"/>
  <exec from="ACCOUNT_ID" key="ACCOUNT_ID" type="copyintolist"/>
  <exec type="loop">
   <exec statement="getCamRnrAccountType" type="select"/>
   <exec text="Initiate R&amp;R" type="comment">
    <exec event="cam.rnrInitiate" ignoreresult="true" type="raisewith"/>
   </exec>
   <exec text="Financing Info (Debt Crystalization)" type="comment">
    <exec event="cam.rnrFinancingInfo" ignoreresult="true" type="raisewith"/>
   </exec>
   <exec text="Notice History" type="comment">
    <exec statement="getCamRnrNoticeHistory" type="select"/>
   </exec>
   <exec text="Approval &amp; Approving Authority" type="comment">
    <exec event="cam.rnrApproval" ignoreresult="true" type="raisewith"/>
   </exec>
  </exec>
 </exec>
</event>
