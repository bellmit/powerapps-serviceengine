<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec type="guard">
  <conditions>
   <condition lhsfield="ACTION_ID" operator="is not empty" rhsvalue=""/>
   <condition lhsfield="ACTION_ID" operator="&lt;&gt;" rhsvalue="parent.ACTION_ID"/>
  </conditions>
  <exec count="1" type="createlist"/>
  <exec statement="getInstructionForEditing" type="select"/>
  <exec type="parallelquery">
   <query value="getStatusForInstructionTermination"/>
   <query value="getActionListTermination"/>
   <query value="getInstructionListTermination"/>
   <query value="getTerminationCondition"/>
   <query value="getAuctionStatusTermination"/>
   <target value="STATUS_LIST"/>
   <target value="ACTION_LIST"/>
   <target value="NEW_INSTRUCTION_LIST"/>
   <target value="TERMINATION_CONDITIONS"/>
   <target value="AUCTION_STATUS_LIST"/>
  </exec>
  <exec argument="HOST_CCRIS_STATUS" query="getHostCCRISStatus" type="query"/>
  <exec type="guard">
   <conditions>
    <condition lhsfield="ACTION_LIST" operator="is not empty" rhsvalue=""/>
   </conditions>
   <exec count="20" to="EMPTY_LIST" type="createlist"/>
   <exec argument="STATUS_LIST" type="loop">
    <exec from="ID" to="ACTION_STATUS_ID" type="copy"/>
   </exec>
   <exec argument="ACTION_LIST" type="loop">
    <exec from="ID" to="CURRENT_ACTION_ID" type="copy"/>
   </exec>
   <exec argument="NEW_INSTRUCTION_LIST" type="loop">
    <exec from="ID" to="NEW_INSTRUCTION_ID" type="copy"/>
   </exec>
   <exec argument="STATUS_LIST" type="loop">
    <exec from="ID" to="INSTRUCTION_STATUS_ID" type="copy"/>
   </exec>
   <exec argument="AUCTION_STATUS_LIST" type="loop">
    <exec from="ID" to="AUCTION_STATUS_ID" type="copy"/>
   </exec>
   <exec from="STATUS_LIST" key="STATUS_LIST" to="EMPTY_LIST" type="copyintolist"/>
   <exec from="ACTION_LIST" key="ACTION_LIST" to="EMPTY_LIST" type="copyintolist"/>
   <exec from="NEW_INSTRUCTION_LIST" key="NEW_INSTRUCTION_LIST" to="EMPTY_LIST" type="copyintolist"/>
   <exec from="AUCTION_STATUS_LIST" key="AUCTION_STATUS_LIST" to="EMPTY_LIST" type="copyintolist"/>
   <exec from="STATUS_LIST" key="STATUS_LIST" to="TERMINATION_CONDITIONS" type="copyintolist"/>
   <exec from="ACTION_LIST" key="ACTION_LIST" to="TERMINATION_CONDITIONS" type="copyintolist"/>
   <exec from="NEW_INSTRUCTION_LIST" key="NEW_INSTRUCTION_LIST" to="TERMINATION_CONDITIONS" type="copyintolist"/>
   <exec from="AUCTION_STATUS_LIST" key="AUCTION_STATUS_LIST" to="TERMINATION_CONDITIONS" type="copyintolist"/>
  </exec>
  <exec argument="ADDITIONAL_DETAILS" query="getAssignedAdditionalDetails" type="query"/>
  <exec from="CODE" key="CODE" type="copyintolist"/>
  <exec from="DESCRIPTION" key="DESCRIPTION" type="copyintolist"/>
  <exec from="TAT_PERIOD" key="TAT_PERIOD" type="copyintolist"/>
  <exec from="VISUAL_ACTION" key="VISUAL_ACTION" type="copyintolist"/>
  <exec from="ADDITIONAL_DETAILS" key="ADDITIONAL_DETAILS" type="copyintolist"/>
  <exec from="DISABLE" key="DISABLE" type="copyintolist"/>
  <exec from="TERMINATION_CONDITIONS" key="TERMINATION_CONDITIONS" type="copyintolist"/>
  <exec from="EMPTY_LIST" key="EMPTY_LIST" type="copyintolist"/>
  <exec from="CONTRACT" key="CONTRACT" type="copyintolist"/>
  <exec from="HOST_CCRIS_STATUS" key="HOST_CCRIS_STATUS" type="copyintolist"/>
  <exec from="HOST_CCRIS_STATUS_ID" key="HOST_CCRIS_STATUS_ID" type="copyintolist"/>
  <exec from="SST" key="SST" type="copyintolist"/>
 </exec>
</event>
