<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="true" type="event">
 <exec count="1" type="createlist"/>
 <exec to="LOGON_USER" type="user"/>
 <exec ceiling="false" endmonth="false" nextweekday="false" startmonth="false" to="CURRENT_TIME" truncate="false" type="date"/>
 <exec statement="insertTeam" type="insert"/>
 <exec type="guard">
  <conditions>
   <condition lhsfield="TEAM_MEMBERS" operator="is not empty" rhsvalue=""/>
  </conditions>
  <exec from="ID" key="TEAM_ID" to="TEAM_MEMBERS" type="copyintolist"/>
  <exec argument="TEAM_MEMBERS" type="loop">
   <exec statement="insertUserTeamRel" type="insert"/>
  </exec>
 </exec>
 <exec message="New team added" target="MESSAGE" type="formattext"/>
 <exec from="MESSAGE" key="MESSAGE" type="copyintolist"/>
 <exec from="TRUE" key="OPEN_MESSAGE" type="copyintolist"/>
 <exec from="TRUE" key="CLOSE" type="copyintolist"/>
 <exec from="TRUE" key="LOAD_TEAMS" type="copyintolist"/>
</event>
