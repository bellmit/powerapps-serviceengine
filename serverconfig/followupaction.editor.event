  <exec count="1" type="createlist"/>
  <exec statement="getInstructionForEditing" type="select"/>
  <exec argument="ADDITIONAL_DETAILS" query="getAssignedAdditionalDetails" type="query"/>
  <exec from="CODE" key="CODE" type="copyintolist"/>
  <exec from="DESCRIPTION" key="DESCRIPTION" type="copyintolist"/>
  <exec from="TAT" key="TAT" type="copyintolist"/>
  <exec from="VISUAL_ACTION" key="VISUAL_ACTION" type="copyintolist"/>
  <exec from="ADDITIONAL_DETAILS" key="ADDITIONAL_DETAILS" type="copyintolist"/>
