<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec type="guard">
  <conditions>
   <condition lhsfield="PROCESS_TYPE_ID" operator="is not empty" rhsvalue=""/>
  </conditions>
  <exec count="1" type="createlist"/>
  <exec type="parallelquery">
   <query value="getDefaultTreatmentStatusType"/>
   <query value="getDefaultTreatmentSubType"/>
   <query value="getTreatmentTemplate"/>
   <query value="getManagementApprovalAccountStatus"/>
   <query value="getManagementApprovalCentre"/>
   <target value="TYPE_STATUS_LIST"/>
   <target value="SUBTYPE_LIST"/>
   <target value="TEMPLATE_LIST"/>
   <target value="ACCOUNT_STATUS_LIST"/>
   <target value="CENTRE_LIST"/>
  </exec>
  <exec type="parallelquery">
   <query value="getManagementApprovalActionType"/>
   <target value="ACTION_TYPE_LIST"/>
  </exec>
  <exec argument="SUBTYPE_LIST" type="loop">
   <exec from="ID" to="SUBTYPE_ID" type="copy"/>
   <exec from="CODE" to="SUBTYPE_CODE" type="copy"/>
  </exec>
  <exec from="ACTION_TYPE_LIST" key="SUBTYPE_ID" target="ACTION_TYPE_LIST" to="SUBTYPE_LIST" type="groupedmerge"/>
  <exec count="100" to="EMPTY_LIST" type="createlist"/>
  <exec from="TYPE_STATUS_LIST" key="TYPE_STATUS_LIST" type="copyintolist"/>
  <exec from="SUBTYPE_LIST" key="SUBTYPE_LIST" type="copyintolist"/>
  <exec from="TEMPLATE_LIST" key="TEMPLATE_LIST" type="copyintolist"/>
  <exec from="CENTRE_LIST" key="CENTRE_LIST" type="copyintolist"/>
  <exec from="ACCOUNT_STATUS_LIST" key="ACCOUNT_STATUS_LIST" type="copyintolist"/>
  <exec from="EMPTY_LIST" key="EMPTY_LIST" type="copyintolist"/>
 </exec>
</event>
