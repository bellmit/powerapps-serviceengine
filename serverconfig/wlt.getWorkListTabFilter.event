  </exec>
  <exec from="MG_WLT_LIST" to="RESULT_LIST" type="copy"/>
 </exec>
 <exec type="guard">
  <conditions>
   <condition lhsfield="MODULE_NAME" operator="=" rhsvalue="OD"/>
  </conditions>
  <exec type="guard">
   <conditions>
    <condition lhsfield="TYPE_CODE" operator="=" rhsvalue="bankuser"/>
   </conditions>
