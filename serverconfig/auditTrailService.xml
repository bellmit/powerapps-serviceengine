<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="auditTrail">

  <parameterMap id="insertAuditLog-pmap" class="map">
<parameter property="REQUEST_TIME" javaType="date" jdbcType="TIMESTAMP"/>
<parameter property="REQUEST_DURATION" javaType="long"/>
<parameter property="USER_ID"/>
<parameter property="MODULE"/>
<parameter property="PC_NAME"/>
<parameter property="AUDIT_STATUS_ID" javaType="long"/>
<parameter property="REMARKS"/>
  </parameterMap>
  <insert id="insertAuditLog" parameterMap="insertAuditLog-pmap">
    insert into PTRAUDIT_LOG
    (REQUEST_TIME, REQUEST_DURATION, USER_ID, MODULE, PC_NAME, AUDIT_STATUS_ID, REMARKS)
    values
    (?, ?, ?, ?, ?, ?, ?)
  </insert>

  </sqlMap>

