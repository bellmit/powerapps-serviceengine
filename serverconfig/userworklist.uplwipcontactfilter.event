<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec text="Filter for contact" type="comment">
  <exec type="guard">
   <conditions>
    <condition lhsfield="WIP_TYPE_DESCRIPTION" operator="=" rhsvalue="Contact"/>
   </conditions>
   <exec type="guard">
    <conditions>
     <condition lhsfield="WIP_SUB_TYPE_DESCRIPTION" operator="=" rhsvalue="Telegram"/>
    </conditions>
    <exec from="TRUE" key="TRUE" to="ACCOUNT_LIST" type="copyintolist"/>
    <exec from="FALSE" key="KEEP" to="ACCOUNT_LIST" type="copyintolist"/>
    <exec argument="ACCOUNT_LIST" type="loop">
     <exec type="guard">
      <conditions>
       <condition lhsfield="TELEGRAM_FILTER" operator="=" rhsvalue="1"/>
      </conditions>
      <exec from="TRUE" to="KEEP" type="copy"/>
     </exec>
    </exec>
    <exec key="KEEP" minuend="ACCOUNT_LIST" subtrahend="FALSE" to="ACCOUNT_LIST" type="remove"/>
   </exec>
   <exec type="guard">
    <conditions>
     <condition lhsfield="WIP_SUB_TYPE_DESCRIPTION" operator="=" rhsvalue="SMS"/>
    </conditions>
    <exec from="TRUE" key="TRUE" to="ACCOUNT_LIST" type="copyintolist"/>
    <exec from="FALSE" key="KEEP" to="ACCOUNT_LIST" type="copyintolist"/>
    <exec argument="ACCOUNT_LIST" type="loop">
     <exec type="guard">
      <conditions>
       <condition lhsfield="SMS_FILTER" operator="=" rhsvalue="1"/>
      </conditions>
      <exec from="TRUE" to="KEEP" type="copy"/>
     </exec>
    </exec>
    <exec key="KEEP" minuend="ACCOUNT_LIST" subtrahend="FALSE" to="ACCOUNT_LIST" type="remove"/>
   </exec>
   <exec type="guard">
    <conditions>
     <condition lhsfield="WIP_SUB_TYPE_DESCRIPTION" operator="=" rhsvalue="Letter"/>
    </conditions>
    <exec from="TRUE" key="TRUE" to="ACCOUNT_LIST" type="copyintolist"/>
    <exec from="FALSE" key="KEEP" to="ACCOUNT_LIST" type="copyintolist"/>
    <exec argument="ACCOUNT_LIST" type="loop">
     <exec type="guard">
      <conditions>
       <condition lhsfield="LETTER_FILTER" operator="=" rhsvalue="1"/>
      </conditions>
      <exec from="TRUE" to="KEEP" type="copy"/>
     </exec>
    </exec>
    <exec key="KEEP" minuend="ACCOUNT_LIST" subtrahend="FALSE" to="ACCOUNT_LIST" type="remove"/>
   </exec>
   <exec type="guard">
    <conditions>
     <condition lhsfield="WIP_SUB_TYPE_DESCRIPTION" operator="=" rhsvalue="Follow up Accounts"/>
    </conditions>
    <exec from="TRUE" key="TRUE" to="ACCOUNT_LIST" type="copyintolist"/>
    <exec from="FALSE" key="KEEP" to="ACCOUNT_LIST" type="copyintolist"/>
    <exec argument="ACCOUNT_LIST" type="loop">
     <exec type="guard">
      <conditions>
       <condition lhsfield="FOLLOWUP_FILTER" operator="=" rhsvalue="1"/>
      </conditions>
      <exec from="TRUE" to="KEEP" type="copy"/>
     </exec>
    </exec>
    <exec key="KEEP" minuend="ACCOUNT_LIST" subtrahend="FALSE" to="ACCOUNT_LIST" type="remove"/>
   </exec>
  </exec>
 </exec>
</event>
