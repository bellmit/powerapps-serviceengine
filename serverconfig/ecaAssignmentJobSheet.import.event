<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec count="1" type="createlist"/>
 <exec to="USER_ID" type="user"/>
 <exec statement="getUserUserType" type="select"/>
 <exec type="guard">
  <conditions>
   <condition lhsfield="SPREADSHEET_LIST" operator="is not empty" rhsvalue=""/>
  </conditions>
  <exec message="ff3333" target="RED" type="formattext"/>
  <exec from="FALSE" key="VALID" to="SPREADSHEET_LIST" type="copyintolist"/>
  <exec from="RED" key="COLOR" to="SPREADSHEET_LIST" type="copyintolist"/>
  <exec from="SPREADSHEET_LIST" to="SPREADSHEET_LIST_COPY" type="copy"/>
  <exec size="999" source="SPREADSHEET_LIST" target="DIVIDED_SPREADSHEET_LIST" type="dividelist"/>
  <exec argument="DIVIDED_SPREADSHEET_LIST" type="loop">
   <exec from="SPREADSHEET_LIST" key="CUSTOMER_PRIMARY_ID" to="CUSTOMERS" type="extract"/>
   <exec from="SPREADSHEET_LIST" key="ACCOUNT_NO" to="ACCOUNTS" type="extract"/>
   <exec type="parallelquery">
    <query value="getEcaJobSheetValidCustomers"/>
    <target value="VALID_ACCOUNT_LIST"/>
   </exec>
   <exec from="VALID_ACCOUNT_LIST" key="ACCOUNT_NO" to="SPREADSHEET_LIST" type="merge"/>
  </exec>
  <exec from="DIVIDED_SPREADSHEET_LIST" key="SPREADSHEET_LIST" to="SPREADSHEET_LIST" type="extract"/>
  <exec argument="SPREADSHEET_LIST" type="loop">
   <exec from="REMARKS" to="NOTE_TEXT" type="copy"/>
   <exec statement="getEcaJobSheetTypeStatus" type="select"/>
   <exec statement="getEcaJobSheetSubtype" type="select"/>
   <exec type="guard">
    <conditions>
     <condition lhsfield="DATE" operator="is not empty" rhsvalue=""/>
     <condition lhsfield="TIME" operator="is not empty" rhsvalue=""/>
    </conditions>
    <exec message="{0} {1}" target="TIMESTAMP" type="formattext">
     <key value="DATE"/>
     <key value="TIME"/>
    </exec>
    <exec statement="getEcaJobSheetTimestamp" type="select"/>
    <exec from="TIMESTAMP" to="ACTION_DATE" type="copy"/>
    <exec from="TIMESTAMP" to="ACTION_TIME" type="copy"/>
   </exec>
   <exec type="guard">
    <conditions>
     <condition lhsfield="PTP_DATE" operator="is not empty" rhsvalue=""/>
    </conditions>
    <exec statement="getEcaJobSheetPtpDate" type="select"/>
   </exec>
  </exec>
 </exec>
 <exec from="SPREADSHEET_LIST" key="SPREADSHEET_LIST" type="copyintolist"/>
</event>
