<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec type="guard">
  <conditions>
   <condition lhsfield="USER_LIST" operator="exists" rhsvalue=""/>
   <condition lhsfield="USER_LIST" operator="is not empty" rhsvalue=""/>
  </conditions>
  <exec list="USER_LIST" to="COUNT_USER_LIST" type="countlist"/>
  <exec from="WLM_USER_LIST_SELECTED" key="USER_ID" to="SELECTED_USER" type="extract"/>
  <exec statement="getBooleanNull" type="select"/>
  <exec from="IS_DEFAULT_USER" key="IS_DEFAULT_USER" to="USER_LIST" type="copyintolist"/>
  <exec type="guard">
   <conditions>
    <condition lhsfield="COUNT_USER_LIST" operator="&lt;&gt;" rhsvalue="0"/>
   </conditions>
   <exec from="WORK_LIST_ID_COPY" to="WORK_LIST_ID" type="copy"/>
   <exec type="transaction">
    <exec recordcount="" statement="updateNullifyWorkListDefaultUser" type="update"/>
   </exec>
   <exec type="transaction">
    <exec recordcount="" statement="updateWorkListDefaultUser" type="update"/>
   </exec>
   <exec argument="DEFAULT_USER_LIST" query="getWorkListDefaultUser" type="query"/>
   <exec from="DEFAULT_USER_LIST" key="USER_ID" to="USER_LIST" type="merge"/>
  </exec>
 </exec>
 <exec from="USER_LIST" type="copy"/>
</event>
