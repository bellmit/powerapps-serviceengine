 </exec>
 <exec argument="RESULT_LIST" type="loop">
  <exec type="guard">
   <conditions>
    <condition lhsfield="IS_EXCLUSIVE" operator="=" rhsvalue="true"/>
   </conditions>
   <exec message="&lt;html&gt;&lt;strong&gt;{0}&lt;/strong&gt;&lt;/html&gt;" target="DESCRIPTION" type="formattext">
    <key value="DESCRIPTION"/>
   </exec>
  </exec>
 </exec>
 <exec type="guard">
  <conditions>
   <condition lhsfield="doNothing" operator="=" rhsvalue="true"/>
  </conditions>
  <exec type="guard">
   <conditions>
    <condition lhsfield="RESULT_LIST" operator="exists" rhsvalue=""/>
   </conditions>
   <exec key="KEEP" minuend="RESULT_LIST" subtrahend="WLT_FALSE" to="RESULT_LIST" type="remove"/>
  </exec>
 </exec>
 <exec from="RESULT_LIST" type="copy"/>
</event>
