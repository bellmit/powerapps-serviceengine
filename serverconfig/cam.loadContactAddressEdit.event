<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec count="1" type="createlist"/>
 <exec from="CONTACT_ID" key="CONTACT_ID" type="copyintolist"/>
 <exec type="loop">
  <exec type="guard">
   <conditions>
    <condition lhsfield="CONTACT_ID" operator="is not empty" rhsvalue=""/>
    <condition lhsfield="CONTACT_ID" operator="&lt;&gt;" rhsvalue="parent.ADDRESS_CONTACT_ID"/>
   </conditions>
   <exec statement="getContactAddressEdit" type="select"/>
  </exec>
  <exec argument="TYPE_LIST" query="getContactAddressTypeRef" type="query"/>
  <exec argument="STATE_LIST" query="getContactStateRef" type="query"/>
  <exec argument="COUNTRY_LIST" query="getContactCountryRef" type="query"/>
  <exec argument="VERIFIED_STATUS_LIST" query="getContactVerifiedStatus" type="query"/>
  <exec from="COUNTRY_LIST" key="COUNTRY_ID" target="COUNTRY_LIST" to="STATE_LIST" type="groupedmerge"/>
 </exec>
</event>
