<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec to="LOGON_USER" type="user"/>
 <exec statement="getLoginUserType" type="select"/>
 <exec type="guard">
  <conditions>
   <condition lhsfield="USER_TYPE_ID" operator="=" rhsvalue="2"/>
  </conditions>
  <exec statement="getLoginUserAgencyId" type="select"/>
 </exec>
 <exec argument="DATA" query="unbilledData" type="query"/>
 <exec from="DATA" key="INSTRUCTION_PROCESS_ID" to="INSTRUCTION_PROCESS_IDS" type="extract"/>
 <exec type="guard">
  <conditions>
   <condition lhsfield="INSTRUCTION_PROCESS_IDS" operator="is not empty" rhsvalue=""/>
  </conditions>
  <exec argument="SUMMARY_DETAILS" query="legalBillingSummaryDetails" type="query"/>
  <exec from="SUMMARY_DETAILS" key="INSTRUCTION_PROCESS_ID" to="DATA" type="merge"/>
 </exec>
 <exec list="DATA" to="TOTAL_RECORD" type="countlist"/>
 <exec type="guard">
  <conditions>
   <condition lhsfield="TOTAL_RECORD" operator="&lt;&gt;" rhsvalue="0"/>
  </conditions>
  <exec count="1" type="createlist"/>
  <exec from="DATA" key="DATA" type="copyintolist"/>
  <exec from="TOTAL_RECORD" key="TOTAL_RECORD" type="copyintolist"/>
  <exec from="TRUE" key="INIT" type="copyintolist"/>
  <exec from="SELECTED" key="SELECTED" type="copyintolist"/>
 </exec>
 <exec type="guard">
  <conditions>
   <condition lhsfield="TOTAL_RECORD" operator="=" rhsvalue="0"/>
  </conditions>
  <exec count="1" type="createlist"/>
  <exec from="ZERO" key="TOTAL_PAGE" type="copyintolist"/>
  <exec from="ZERO" key="TOTAL_RECORD" type="copyintolist"/>
  <exec from="ZERO" key="CURRENT_PAGE" type="copyintolist"/>
  <exec message="No record found!!!" target="MESSAGE" type="formattext"/>
  <exec from="MESSAGE" key="MESSAGE" type="copyintolist"/>
  <exec from="TRUE" key="OPEN_MESSAGE" type="copyintolist"/>
 </exec>
</event>
