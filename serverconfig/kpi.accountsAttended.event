<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec type="parallelquery">
  <query value="getKpiNoOfCallsMadeByUser"/>
  <query value="getKpiNoOfAccountsAttendedByUser"/>
  <target value="NO_OF_CALLS_MADE"/>
  <target value="NO_OF_ACCOUNTS_ATTENDED"/>
 </exec>
 <exec count="1" type="createlist"/>
 <exec from="NO_OF_ACCOUNTS_ATTENDED" key="USER" to="NO_OF_CALLS_MADE" type="merge"/>
 <exec from="TRUE" key="VIEW_ACCOUNT_DETAILS" to="NO_OF_CALLS_MADE" type="copyintolist"/>
 <exec key="TOTAL_CALLS" list="NO_OF_CALLS_MADE" to="TOTAL_CALLS" type="sumlist"/>
 <exec key="TOTAL_ACCOUNTS" list="NO_OF_CALLS_MADE" to="TOTAL_ACCOUNTS" type="sumlist"/>
 <exec list="NO_OF_CALLS_MADE" to="TOTAL_USERS" type="countlist"/>
 <exec count="1" to="TOTAL" type="createlist"/>
 <exec from="TOTAL_ACCOUNTS" key="TOTAL_ACCOUNTS" to="TOTAL" type="copyintolist"/>
 <exec from="TOTAL_CALLS" key="TOTAL_CALLS" to="TOTAL" type="copyintolist"/>
 <exec message="Total Users :{0}" target="USER" type="formattext">
  <key value="TOTAL_USERS"/>
 </exec>
 <exec from="USER" key="USER" to="TOTAL" type="copyintolist"/>
 <exec message="EAEAEA" target="COLOR" type="formattext"/>
 <exec from="COLOR" key="COLOR" to="TOTAL" type="copyintolist"/>
 <exec from="TOTAL" to="NO_OF_CALLS_MADE" type="append"/>
 <exec from="NO_OF_CALLS_MADE" key="DATA" type="copyintolist"/>
</event>
