<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="true" type="event">
 <exec from="SELECTED_DATA" key="AGENCY_ID" to="AGENCIES" type="extract"/>
 <exec argument="ACCOUNT_LIST" query="streamVerificationDataForCreateAssignment" type="query"/>
 <exec ceiling="false" endmonth="false" nextweekday="false" startmonth="false" to="CURRENT_TIME" truncate="false" type="date"/>
 <exec to="LOGON_USER" type="user"/>
 <exec from="CURRENT_TIME" key="CURRENT_TIME" to="ACCOUNT_LIST" type="copyintolist"/>
 <exec from="LOGON_USER" key="LOGON_USER" to="ACCOUNT_LIST" type="copyintolist"/>
 <exec from="REMARKS" key="REMARKS" to="ACCOUNT_LIST" type="copyintolist"/>
 <exec argument="ACCOUNT_LIST" type="loop">
  <exec statement="createAssignmentViaVerification" type="insert"/>
  <exec statement="deleteVerificationById" type="delete"/>
  <exec type="guard">
   <conditions>
    <condition lhsfield="PROCESS_TYPE_ID" operator="=" rhsvalue="19"/>
   </conditions>
   <exec statement="submitRecommendation" type="insert"/>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="PROCESS_TYPE_ID" operator="&lt;&gt;" rhsvalue="19"/>
   </conditions>
   <exec from="ID" to="AGENCY_ASSIGNMENT_ID" type="copy"/>
   <exec statement="pendingAcknowledgement" type="insert"/>
   <exec statement="monitorAssignment" type="insert"/>
  </exec>
  <exec statement="clearRejectedOnCreateAction" type="delete"/>
  <exec statement="clearQueueOnCreateAction" type="delete"/>
  <exec recordcount="ACCOUNT_LAST_ASSIGNMENT_SUMMARY_UPDATE_COUNT" statement="updateAccountLastAssignmentSummary" type="update"/>
  <exec type="guard">
   <conditions>
    <condition lhsfield="ACCOUNT_LAST_ASSIGNMENT_SUMMARY_UPDATE_COUNT" operator="=" rhsvalue="0"/>
   </conditions>
   <exec statement="insertAccountLastAssignmentSummary" type="insert"/>
  </exec>
 </exec>
 <exec count="1" type="createlist"/>
 <exec message="Sent for recommendation" target="MESSAGE" type="formattext"/>
 <exec from="TRUE" key="CLOSE_REMARKS" type="copyintolist"/>
 <exec from="TRUE" key="INIT" type="copyintolist"/>
 <exec from="TRUE" key="OPEN_MESSAGE" type="copyintolist"/>
 <exec from="MESSAGE" key="MESSAGE" type="copyintolist"/>
 <exec from="TRUE" key="LOAD_DATA" type="copyintolist"/>
</event>
