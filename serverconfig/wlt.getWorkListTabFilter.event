  </exec>
  <exec from="OD_WLT_LIST" to="RESULT_LIST" type="copy"/>
 </exec>
 <exec type="guard">
  <conditions>
   <condition lhsfield="MODULE_NAME" operator="=" rhsvalue="RNR"/>
  </conditions>
  <exec type="guard">
   <conditions>
    <condition lhsfield="TYPE_CODE" operator="=" rhsvalue="bankuser"/>
   </conditions>
   <exec argument="RNR_WLT_LIST" query="getWorkListTabsReferenceRnR" type="query"/>
  </exec>
  <exec from="RNR_WLT_LIST" to="RESULT_LIST" type="copy"/>
 </exec>
 <exec type="guard">
