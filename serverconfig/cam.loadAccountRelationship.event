<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec type="guard">
  <conditions>
   <condition lhsfield="CUSTOMER_ID" operator="is not empty" rhsvalue=""/>
   <condition lhsfield="CUSTOMER_ID" operator="&lt;&gt;" rhsvalue="parent.CUSTOMER_ID"/>
   <condition lhsfield="ACCOUNT_ID" operator="is not empty" rhsvalue=""/>
   <condition lhsfield="ACCOUNT_ID" operator="&lt;&gt;" rhsvalue="parent.ACCOUNT_ID"/>
  </conditions>
  <exec count="1" type="createlist"/>
  <exec from="CUSTOMER_ID" key="CUSTOMER_ID" type="copyintolist"/>
  <exec type="loop">
   <exec statement="selectOne" type="select"/>
   <exec statement="getTrue" type="select"/>
   <exec argument="CURRENT_FINANCIAL_INFORMATION" query="getAccountRelationshipCurrentFinancialInformation" type="query"/>
   <exec from="TRUE" key="TRUE" to="CURRENT_FINANCIAL_INFORMATION" type="copyintolist"/>
   <exec from="ZERO" to="SELECTED_INDEX1" type="copy"/>
   <exec from="ZERO" to="SELECTED_INDEX2" type="copy"/>
   <exec from="ZERO" to="SELECTED_INDEX3" type="copy"/>
  </exec>
 </exec>
</event>
