<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec count="1" type="createlist"/>
 <exec statement="getParameterForEditing" type="select"/>
 <exec message="SHOW" target="SHOW" type="formattext"/>
 <exec message="HIDE" target="HIDE" type="formattext"/>
 <exec from="HIDE" to="_STANDARD_EDITOR" type="copy"/>
 <exec from="HIDE" to="_CUSTOM_EDITOR" type="copy"/>
 <exec from="HIDE" to="_DEFAULT" type="copy"/>
 <exec from="HIDE" to="_ABORT_REASON" type="copy"/>
 <exec from="HIDE" to="_AGENCY_TAT" type="copy"/>
 <exec from="HIDE" to="_BILLING_TAT" type="copy"/>
 <exec from="HIDE" to="_ABORT_PERIOD" type="copy"/>
 <exec from="HIDE" to="_AGENCY_CONTRACT_TAG" type="copy"/>
 <exec type="guard">
  <conditions>
   <condition lhsfield="IS_STANDARD" operator="=" rhsvalue="true"/>
  </conditions>
  <exec from="SHOW" to="_STANDARD_EDITOR" type="copy"/>
  <exec from="TRUE" key="LOAD_STANDARD_EDITOR" type="copyintolist"/>
  <exec argument="REFERENCE" query="getStandardTableContent" type="query"/>
 </exec>
 <exec type="guard">
  <conditions>
   <condition lhsfield="IS_STANDARD" operator="&lt;&gt;" rhsvalue="true"/>
  </conditions>
  <exec from="SHOW" to="_CUSTOM_EDITOR" type="copy"/>
  <exec type="guard">
   <conditions>
    <condition lhsfield="TABLE_NAME" operator="=" rhsvalue="PTRLEGAL_ABORT_REASON_REF"/>
   </conditions>
   <exec type="parallelquery">
    <query value="getLegalAbortReasonECAContent"/>
    <query value="getAbortReasonECARef"/>
    <target value="REFERENCE"/>
    <target value="SUBTYPES"/>
   </exec>
   <exec argument="SUBTYPES" type="loop">
    <exec from="ID" to="SUBTYPE_ID" type="copy"/>
   </exec>
   <exec from="SUBTYPES" key="SUBTYPES" to="REFERENCE" type="copyintolist"/>
   <exec from="TRUE" key="ABORT_REASON" type="copyintolist"/>
   <exec from="SHOW" to="_ABORT_REASON" type="copy"/>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="TABLE_NAME" operator="=" rhsvalue="PTRAGENCY_TAT_REF"/>
   </conditions>
   <exec type="parallelquery">
    <query value="getAgencyTatECAContent"/>
    <target value="REFERENCE"/>
   </exec>
   <exec from="TRUE" key="AGENCY_TAT" type="copyintolist"/>
   <exec from="SHOW" to="_AGENCY_TAT" type="copy"/>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="TABLE_NAME" operator="=" rhsvalue="PTRASSIGNMENT_ABORT_PERIOD_REF"/>
   </conditions>
   <exec type="parallelquery">
    <query value="getAssignmentAbortPeriodECAContent"/>
    <target value="REFERENCE"/>
   </exec>
   <exec from="TRUE" key="ABORT_PERIOD" type="copyintolist"/>
   <exec from="SHOW" to="_ABORT_PERIOD" type="copy"/>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="TABLE_NAME" operator="=" rhsvalue="PTRBILLING_TAT_REF"/>
   </conditions>
   <exec type="parallelquery">
    <query value="getBillingTatECAContent"/>
    <target value="REFERENCE"/>
   </exec>
   <exec from="TRUE" key="BILLING_TAT" type="copyintolist"/>
   <exec from="SHOW" to="_BILLING_TAT" type="copy"/>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="TABLE_NAME" operator="=" rhsvalue="PTRCONTRACT_TAG_REF"/>
   </conditions>
   <exec type="parallelquery">
    <query value="getContractTagECAContent"/>
    <target value="REFERENCE"/>
   </exec>
   <exec from="TRUE" key="AGENCY_CONTRACT_TAG" type="copyintolist"/>
   <exec from="SHOW" to="_AGENCY_CONTRACT_TAG" type="copy"/>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="TABLE_NAME" operator="=" rhsvalue="PTRAGENCY_INVOICE_NUMBER_REF"/>
   </conditions>
   <exec from="SHOW" to="_INVOICE_STARTING_NUMBER" type="copy"/>
   <exec from="TRUE" key="INVOICE_STARTING_NUMBER" type="copyintolist"/>
   <exec argument="REFERENCE" query="getECAInvoiceNumberRef" type="query"/>
  </exec>
 </exec>
 <exec from="_CUSTOM_EDITOR" key="_CUSTOM_EDITOR" type="copyintolist"/>
 <exec from="_STANDARD_EDITOR" key="_STANDARD_EDITOR" type="copyintolist"/>
 <exec from="_DEFAULT" key="_DEFAULT" type="copyintolist"/>
 <exec from="_DEFAULT" key="_DEFAULT" type="copyintolist"/>
 <exec from="TABLE_NAME" key="TABLE_NAME" type="copyintolist"/>
 <exec from="IS_STANDARD" key="IS_STANDARD" type="copyintolist"/>
 <exec from="REFERENCE" key="REFERENCE" type="copyintolist"/>
 <exec from="SEQUENCE_NAME" key="SEQUENCE_NAME" type="copyintolist"/>
 <exec from="ALLOW_ADD" key="ALLOW_ADD" type="copyintolist"/>
 <exec from="CODE_PARAMETER" key="CODE_PARAMETER" type="copyintolist"/>
 <exec from="_ABORT_REASON" key="_ABORT_REASON" type="copyintolist"/>
 <exec from="_AGENCY_TAT" key="_AGENCY_TAT" type="copyintolist"/>
 <exec from="_BILLING_TAT" key="_BILLING_TAT" type="copyintolist"/>
 <exec from="_ABORT_PERIOD" key="_ABORT_PERIOD" type="copyintolist"/>
 <exec from="_AGENCY_CONTRACT_TAG" key="_AGENCY_CONTRACT_TAG" type="copyintolist"/>
 <exec from="_INVOICE_STARTING_NUMBER" key="_INVOICE_STARTING_NUMBER" type="copyintolist"/>
</event>
