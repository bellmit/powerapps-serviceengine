<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec text="Filter for management approval" type="comment">
  <exec type="guard">
   <conditions>
    <condition lhsfield="WIP_TYPE_DESCRIPTION" operator="=" rhsvalue="Management Approval"/>
   </conditions>
   <exec type="guard">
    <conditions>
     <condition lhsfield="WIP_SUB_TYPE_DESCRIPTION" operator="=" rhsvalue="Pending Legal Approval"/>
    </conditions>
    <exec from="TRUE" key="TRUE" to="ACCOUNT_LIST" type="copyintolist"/>
    <exec from="FALSE" key="KEEP" to="ACCOUNT_LIST" type="copyintolist"/>
    <exec argument="ACCOUNT_LIST" type="loop">
     <exec type="guard">
      <conditions>
       <condition lhsfield="MANAGEMENT_APPROVAL_FILTER" operator="=" rhsvalue="1"/>
      </conditions>
      <exec from="TRUE" to="KEEP" type="copy"/>
     </exec>
    </exec>
    <exec key="KEEP" minuend="ACCOUNT_LIST" subtrahend="FALSE" to="ACCOUNT_LIST" type="remove"/>
   </exec>
   <exec type="guard">
    <conditions>
     <condition lhsfield="WIP_SUB_TYPE_DESCRIPTION" operator="=" rhsvalue="Success Legal Approval"/>
    </conditions>
    <exec from="TRUE" key="TRUE" to="ACCOUNT_LIST" type="copyintolist"/>
    <exec from="FALSE" key="KEEP" to="ACCOUNT_LIST" type="copyintolist"/>
    <exec argument="ACCOUNT_LIST" type="loop">
     <exec type="guard">
      <conditions>
       <condition lhsfield="MANAGEMENT_APPROVAL_FILTER" operator="=" rhsvalue="2"/>
      </conditions>
      <exec from="TRUE" to="KEEP" type="copy"/>
     </exec>
    </exec>
    <exec key="KEEP" minuend="ACCOUNT_LIST" subtrahend="FALSE" to="ACCOUNT_LIST" type="remove"/>
   </exec>
   <exec type="guard">
    <conditions>
     <condition lhsfield="WIP_SUB_TYPE_DESCRIPTION" operator="=" rhsvalue="Cancel Legal Approval"/>
    </conditions>
    <exec from="TRUE" key="TRUE" to="ACCOUNT_LIST" type="copyintolist"/>
    <exec from="FALSE" key="KEEP" to="ACCOUNT_LIST" type="copyintolist"/>
    <exec argument="ACCOUNT_LIST" type="loop">
     <exec type="guard">
      <conditions>
       <condition lhsfield="MANAGEMENT_APPROVAL_FILTER" operator="=" rhsvalue="3"/>
      </conditions>
      <exec from="TRUE" to="KEEP" type="copy"/>
     </exec>
    </exec>
    <exec key="KEEP" minuend="ACCOUNT_LIST" subtrahend="FALSE" to="ACCOUNT_LIST" type="remove"/>
   </exec>
  </exec>
 </exec>
</event>
