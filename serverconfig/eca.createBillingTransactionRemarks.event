<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="true" type="event">
 <exec count="1" type="createlist"/>
 <exec ceiling="false" endmonth="false" nextweekday="false" startmonth="false" to="CURRENT_TIME" truncate="false" type="date"/>
 <exec to="LOGON_USER" type="user"/>
 <exec statement="getLoginUserType" type="select"/>
 <exec type="guard">
  <conditions>
   <condition lhsfield="USER_TYPE_ID" operator="=" rhsvalue="2"/>
  </conditions>
  <exec recordcount="" statement="updateEcaBillingTransactionAppealFromAgency" type="update"/>
 </exec>
 <exec type="guard">
  <conditions>
   <condition lhsfield="USER_TYPE_ID" operator="&lt;&gt;" rhsvalue="2"/>
  </conditions>
  <exec recordcount="" statement="updateEcaBillingTransactionRemarks" type="update"/>
 </exec>
 <exec from="TRUE" key="CLOSE" type="copyintolist"/>
</event>
