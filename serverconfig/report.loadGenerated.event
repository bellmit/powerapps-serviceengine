<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec count="1" type="createlist"/>
 <exec argument="REPORTS" query="getAllReports" type="query"/>
 <exec count="5" to="PERIODS" type="createlist"/>
 <exec argument="PERIODS" index="ID" type="loop">
  <exec message="HIDE" target="_RANGE" type="formattext"/>
  <exec type="guard">
   <conditions>
    <condition lhsfield="ID" operator="=" rhsvalue="1"/>
   </conditions>
   <exec message="Today" target="DESCRIPTION" type="formattext"/>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="ID" operator="=" rhsvalue="2"/>
   </conditions>
   <exec message="7 Days Ago" target="DESCRIPTION" type="formattext"/>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="ID" operator="=" rhsvalue="3"/>
   </conditions>
   <exec message="A Month Ago" target="DESCRIPTION" type="formattext"/>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="ID" operator="=" rhsvalue="4"/>
   </conditions>
   <exec message="A Year Ago" target="DESCRIPTION" type="formattext"/>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="ID" operator="=" rhsvalue="5"/>
   </conditions>
   <exec message="Specify Range" target="DESCRIPTION" type="formattext"/>
   <exec message="SHOW" target="_RANGE" type="formattext"/>
  </exec>
 </exec>
 <exec from="PERIODS" key="PERIODS" type="copyintolist"/>
 <exec from="REPORTS" key="REPORTS" type="copyintolist"/>
</event>
