<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="true" type="event">
 <exec count="1" type="createlist"/>
 <exec ceiling="false" endmonth="false" nextweekday="false" startmonth="false" to="CURRENT_TIME" truncate="false" type="date"/>
 <exec to="LOGON_USER" type="user"/>
 <exec type="guard">
  <conditions>
   <condition lhsfield="AGENCY_ID" operator="is not empty" rhsvalue=""/>
  </conditions>
  <exec recordcount="" statement="updateEcaAgency" type="update"/>
  <exec message="Agency updated" target="MESSAGE" type="formattext"/>
 </exec>
 <exec type="guard">
  <conditions>
   <condition lhsfield="AGENCY_ID" operator="is empty" rhsvalue=""/>
  </conditions>
  <exec statement="insertEcaAgency" type="insert"/>
  <exec message="New agency added" target="MESSAGE" type="formattext"/>
 </exec>
 <exec type="guard">
  <conditions>
   <condition lhsfield="INVOICE_GENERATION_DAY" operator="is not empty" rhsvalue=""/>
  </conditions>
  <exec type="guard">
   <conditions>
    <condition lhsfield="AGENCY_ID" operator="is empty" rhsvalue=""/>
   </conditions>
   <exec from="ID" to="AGENCY_ID" type="copy"/>
  </exec>
  <exec statement="insertAgencyInvoiceDayHistory" type="insert"/>
 </exec>
 <exec from="TRUE" key="CLOSE" type="copyintolist"/>
 <exec from="MESSAGE" key="MESSAGE" type="copyintolist"/>
 <exec from="TRUE" key="OPEN_MESSAGE" type="copyintolist"/>
</event>
