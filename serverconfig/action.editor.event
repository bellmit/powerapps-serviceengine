  <exec from="CODE" key="CODE" type="copyintolist"/>
  <exec from="DESCRIPTION" key="DESCRIPTION" type="copyintolist"/>
  <exec from="TAT" key="TAT" type="copyintolist"/>
  <exec from="VISUAL_ACTION" key="VISUAL_ACTION" type="copyintolist"/>
  <exec from="ADDITIONAL_DETAILS" key="ADDITIONAL_DETAILS" type="copyintolist"/>
  <exec from="DISABLE" key="DISABLE" type="copyintolist"/>
  <exec from="INSTRUCTION_ID" key="INSTRUCTION_ID" type="copyintolist"/>
  <exec from="STAGE_ID" key="STAGE_ID" type="copyintolist"/>
  <exec from="BILLING_STAGE_ID" key="BILLING_STAGE_ID" type="copyintolist"/>
  <exec from="CATEGORY_ID" key="CATEGORY_ID" type="copyintolist"/>
  <exec from="INTERNAL_ACTION" key="INTERNAL_ACTION" type="copyintolist"/>
  <exec from="CONTRACT" key="CONTRACT" type="copyintolist"/>
  <exec from="DISBURSEMENT" key="DISBURSEMENT" type="copyintolist"/>
  <exec from="EXPENSE" key="EXPENSE" type="copyintolist"/>
  <exec from="BILLING_MILESTONE" key="BILLING_MILESTONE" type="copyintolist"/>
  <exec from="EXPENSES" key="EXPENSES" type="copyintolist"/>
