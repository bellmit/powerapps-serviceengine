<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec type="guard">
  <conditions>
   <condition lhsfield="CONFIRM_UPDATE" operator="=" rhsvalue="true"/>
  </conditions>
  <exec event="wlg.executeUser" ignoreresult="true" type="raise"/>
  <exec message="Execution Completed" target="MESSAGE" type="formattext"/>
 </exec>
 <exec type="guard">
  <conditions>
   <condition lhsfield="CONFIRM_UPDATE" operator="=" rhsvalue="false"/>
  </conditions>
  <exec message="Execution Cancelled" target="MESSAGE" type="formattext"/>
 </exec>
 <exec count="1" type="createlist"/>
 <exec from="TRUE" key="OPEN_MESSAGE" type="copyintolist"/>
 <exec from="MESSAGE" key="MESSAGE" type="copyintolist"/>
</event>
