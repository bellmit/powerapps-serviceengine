<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec type="guard">
  <conditions>
   <condition lhsfield="IS_NEW_TREATMENT" operator="=" rhsvalue="true"/>
  </conditions>
  <exec statement="insertTreatmentEmploymentMaintenance" type="insert"/>
 </exec>
 <exec type="guard">
  <conditions>
   <condition lhsfield="IS_NEW_TREATMENT" operator="&lt;&gt;" rhsvalue="true"/>
  </conditions>
  <exec recordcount="" statement="updateTreatmentEmploymentMaintenance" type="update"/>
 </exec>
 <exec type="guard">
  <conditions>
   <condition lhsfield="STATUS_ID" operator="=" rhsvalue="15004"/>
  </conditions>
  <exec recordcount="UPDATE_COUNT" statement="updateCustomerEmploymentMaintenance" type="update"/>
  <exec type="guard">
   <conditions>
    <condition lhsfield="UPDATE_COUNT" operator="=" rhsvalue="0"/>
   </conditions>
   <exec statement="insertCustomerEmploymentMaintenance" type="insert"/>
  </exec>
 </exec>
</event>
