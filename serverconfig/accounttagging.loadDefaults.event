<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec count="1" type="createlist"/>
 <exec type="parallelquery">
  <query value="getDefaultTreatmentStatusType"/>
  <query value="getDefaultTreatmentSubTypeAccountTagging"/>
  <query value="genericList"/>
  <target value="TYPE_STATUS_LIST"/>
  <target value="SUBTYPE_LIST"/>
  <target value="GENERIC_TAGS"/>
 </exec>
 <exec argument="SUBTYPE_LIST" type="loop">
  <exec event="accounttagging.approvalAccess" ignoreresult="true" type="raise"/>
 </exec>
 <exec from="TYPE_STATUS_LIST" key="TYPE_STATUS_LIST" type="copyintolist"/>
 <exec from="SUBTYPE_LIST" key="SUBTYPE_LIST" type="copyintolist"/>
 <exec from="GENERIC_TAGS" key="GENERIC_TAGS" type="copyintolist"/>
 <exec type="guard">
  <conditions>
   <condition lhsfield="ACCOUNT_ID" operator="is not empty" rhsvalue=""/>
   <condition lhsfield="ACCOUNT_ID" operator="&lt;&gt;" rhsvalue="account.ACCOUNT_ID"/>
  </conditions>
  <exec statement="getLatestAccountTaggingDataForTreatment" type="select"/>
 </exec>
 <exec type="guard">
  <conditions>
   <condition lhsfield="ID" operator="is not empty" rhsvalue=""/>
  </conditions>
  <exec statement="getTreatmentAccountTaggingApprovalCode" type="select"/>
  <exec type="guard">
   <conditions>
    <condition lhsfield="APPROVAL_CODE" operator="=" rhsvalue="AT_AKPK"/>
   </conditions>
   <exec statement="getTreatmentAkpkInfo" type="select"/>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="APPROVAL_CODE" operator="=" rhsvalue="AT_BILLING"/>
   </conditions>
   <exec statement="getTreatmentBillingInfo" type="select"/>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="APPROVAL_CODE" operator="=" rhsvalue="AT_DEBT_SALE"/>
   </conditions>
   <exec statement="getTreatmentDebtSaleInfo" type="select"/>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="APPROVAL_CODE" operator="=" rhsvalue="AT_ECU"/>
   </conditions>
   <exec statement="getTreatmentEcuInfo" type="select"/>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="APPROVAL_CODE" operator="=" rhsvalue="AT_LITIGATION"/>
   </conditions>
   <exec statement="getTreatmentLitigationInfo" type="select"/>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="APPROVAL_CODE" operator="=" rhsvalue="AT_MIS"/>
   </conditions>
   <exec statement="getTreatmentMisInfo" type="select"/>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="APPROVAL_CODE" operator="=" rhsvalue="AT_PRODUCT_OWNER"/>
   </conditions>
   <exec statement="getTreatmentProductOwnerInfo" type="select"/>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="APPROVAL_CODE" operator="=" rhsvalue="AT_TAKAFUL"/>
   </conditions>
   <exec statement="getTreatmentTakafulInfo" type="select"/>
  </exec>
 </exec>
 <exec from="AKPK" key="AKPK" type="copyintolist"/>
 <exec from="BLD" key="BLD" type="copyintolist"/>
 <exec from="REFUND_OA_DEPOSIT" key="REFUND_OA_DEPOSIT" type="copyintolist"/>
 <exec from="REFUND_COURT_DEPOSIT" key="REFUND_COURT_DEPOSIT" type="copyintolist"/>
 <exec from="DEBT_SALES" key="DEBT_SALES" type="copyintolist"/>
 <exec from="NEW_DISBURSEMENT" key="NEW_DISBURSEMENT" type="copyintolist"/>
 <exec from="BANKRUPT" key="BANKRUPT" type="copyintolist"/>
 <exec from="SHORTFALL" key="SHORTFALL" type="copyintolist"/>
 <exec from="PENDING_BPP" key="PENDING_BPP" type="copyintolist"/>
 <exec from="AUCTION" key="AUCTION" type="copyintolist"/>
 <exec from="AUCTION_RECEIVED_DEPOSIT" key="AUCTION_RECEIVED_DEPOSIT" type="copyintolist"/>
 <exec from="AUCTION_RECEIVED_BALANCE" key="AUCTION_RECEIVED_BALANCE" type="copyintolist"/>
 <exec from="EX_STAFF" key="EX_STAFF" type="copyintolist"/>
