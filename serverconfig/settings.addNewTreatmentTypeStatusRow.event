<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec count="1" to="NEW_ROW" type="createlist"/>
 <exec type="parallelquery">
  <query value="getTreatmentStatusRef"/>
  <query value="getTreatmentTypeRef"/>
  <query value="getCallSubtypeRef"/>
  <target value="STATUSES"/>
  <target value="TYPES"/>
  <target value="SUBTYPES"/>
 </exec>
 <exec argument="STATUSES" type="loop">
  <exec from="ID" to="STATUS_ID" type="copy"/>
 </exec>
 <exec argument="TYPES" type="loop">
  <exec from="ID" to="TYPE_ID" type="copy"/>
 </exec>
 <exec argument="SUBTYPES" type="loop">
  <exec from="ID" to="SUBTYPE_ID" type="copy"/>
 </exec>
 <exec from="STATUSES" key="STATUSES" to="NEW_ROW" type="copyintolist"/>
 <exec from="TYPES" key="TYPES" to="NEW_ROW" type="copyintolist"/>
 <exec from="SUBTYPES" key="SUBTYPES" to="NEW_ROW" type="copyintolist"/>
 <exec from="TRUE" key="ALLOW_DELETE" to="NEW_ROW" type="copyintolist"/>
 <exec from="NEW_ROW" to="REFERENCE" type="append"/>
 <exec from="REFERENCE" type="copy"/>
</event>
