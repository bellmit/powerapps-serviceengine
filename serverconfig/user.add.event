<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="true" type="event">
 <exec count="1" type="createlist"/>
 <exec ceiling="false" endmonth="false" nextweekday="false" startmonth="false" to="CURRENT_TIME" truncate="false" type="date"/>
 <exec to="LOGON_USER" type="user"/>
 <exec password="PASSWORD" type="hashpassword" userId="USER_ID"/>
 <exec statement="insertUser" type="insert"/>
 <exec statement="deleteUserAssignedRoles" type="delete"/>
 <exec from="USER_ID" key="USER_ID" to="ASSIGNED_ROLES" type="copyintolist"/>
 <exec argument="ASSIGNED_ROLES" type="loop">
  <exec statement="insertUserAssignedRoles" type="insert"/>
 </exec>
 <exec message="New user added" target="MESSAGE" type="formattext"/>
 <exec from="MESSAGE" key="MESSAGE" type="copyintolist"/>
 <exec from="TRUE" key="OPEN_MESSAGE" type="copyintolist"/>
 <exec from="TRUE" key="CLOSE" type="copyintolist"/>
 <exec from="TRUE" key="LOAD_USERS" type="copyintolist"/>
</event>
