<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec count="1" type="createlist"/>
 <exec from="AGENCY_ID" key="AGENCY_ID" type="copyintolist"/>
 <exec from="DATE_ID" key="ID" type="copyintolist"/>
 <exec type="loop">
  <exec message="Add Tentative Date" target="TITLE" type="formattext"/>
  <exec type="guard">
   <conditions>
    <condition lhsfield="ID" operator="is not empty" rhsvalue=""/>
    <condition lhsfield="AGENCY_ID" operator="is not empty" rhsvalue=""/>
   </conditions>
   <exec statement="getAuctionSetupDateForEditing" type="select"/>
   <exec message="Edit Tentative Date" target="TITLE" type="formattext"/>
  </exec>
 </exec>
</event>
