<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec from="USER_TYPE_ID" to="EMPLOYEE_TYPE_ID" type="copy"/>
 <exec type="guard">
  <conditions>
   <condition lhsfield="EMPLOYEE_TYPE_ID" operator="is not empty" rhsvalue=""/>
  </conditions>
  <exec argument="EMPLOYEES" query="getEmployeeForUserEditing" type="query"/>
  <exec argument="EMPLOYEES" type="loop">
   <exec type="guard">
    <conditions>
     <condition lhsfield="FIRST_NAME" operator="is not empty" rhsvalue=""/>
    </conditions>
    <exec message="{0}" target="NAME" type="formattext">
     <key value="FIRST_NAME"/>
    </exec>
   </exec>
   <exec type="guard">
    <conditions>
     <condition lhsfield="MIDDLE_NAME" operator="is not empty" rhsvalue=""/>
    </conditions>
    <exec message="{0} {1}" target="NAME" type="formattext">
     <key value="NAME"/>
     <key value="MIDDLE_NAME"/>
    </exec>
   </exec>
   <exec type="guard">
    <conditions>
     <condition lhsfield="LAST_NAME" operator="is not empty" rhsvalue=""/>
    </conditions>
    <exec message="{0} {1}" target="NAME" type="formattext">
     <key value="NAME"/>
     <key value="LAST_NAME"/>
    </exec>
   </exec>
   <exec type="guard">
    <conditions>
     <condition lhsfield="NAME" operator="is empty" rhsvalue=""/>
    </conditions>
    <exec from="EMPLOYEE_ID" to="NAME" type="copy"/>
   </exec>
   <exec message="{0} - {1}" target="DESCRIPTION" type="formattext">
    <key value="EMPLOYEE_ID"/>
    <key value="NAME"/>
   </exec>
  </exec>
  <exec type="guard">
   <conditions>
    <condition lhsfield="EMPLOYEES" operator="is empty" rhsvalue=""/>
   </conditions>
   <exec argument="EMPLOYEES" query="getEmptyEmployeeDefault" type="query"/>
  </exec>
 </exec>
</event>
