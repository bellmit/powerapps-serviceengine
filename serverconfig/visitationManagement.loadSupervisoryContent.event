<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec count="1" type="createlist"/>
 <exec to="USER_ID" type="user"/>
 <exec from="MODULE_ID" key="MODULE_ID" type="copyintolist"/>
 <exec from="USER_ID" key="USER_ID" type="copyintolist"/>
 <exec from="TRUE" key="TRUE" type="copyintolist"/>
 <exec type="loop">
  <exec statement="getFiveHundred" type="select"/>
  <exec argument="TYPE_STATUSES" query="getVmsTypeStatuses" type="query"/>
  <exec argument="INSTRUCTION_TYPES" query="getVmsInstructionTypes" type="query"/>
  <exec argument="STATES" query="getVmsStates" type="query"/>
  <exec from="TRUE" key="TRUE" to="INSTRUCTION_TYPES" type="copyintolist"/>
  <exec argument="INSTRUCTION_TYPES" type="loop">
   <exec message="HIDE" target="_VISITATION_STAFF" type="formattext"/>
   <exec message="HIDE" target="_SEARCH_PARAM" type="formattext"/>
   <exec type="guard">
    <conditions>
     <condition lhsfield="ID" operator="=" rhsvalue="1"/>
    </conditions>
    <exec message="SHOW" target="_SEARCH_PARAM" type="formattext"/>
   </exec>
   <exec type="guard">
    <conditions>
     <condition lhsfield="ID" operator="=" rhsvalue="2"/>
    </conditions>
    <exec message="SHOW" target="_SEARCH_PARAM" type="formattext"/>
    <exec message="SHOW" target="_VISITATION_STAFF" type="formattext"/>
   </exec>
   <exec type="guard">
    <conditions>
     <condition lhsfield="ID" operator="=" rhsvalue="3"/>
    </conditions>
    <exec message="SHOW" target="_SEARCH_PARAM" type="formattext"/>
    <exec message="SHOW" target="_VISITATION_STAFF" type="formattext"/>
   </exec>
  </exec>
  <exec from="TRUE" key="LOAD_AREAS" to="STATES" type="copyintolist"/>
  <exec from="TRUE" to="LOAD_AREAS" type="copy"/>
  <exec from="FIVE_HUNDRED" to="LIMIT_RESULT" type="copy"/>
 </exec>
</event>
