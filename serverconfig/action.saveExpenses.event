<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec statement="clearActionExpenses" type="delete"/>
 <exec type="guard">
  <conditions>
   <condition lhsfield="EXPENSES" operator="is not empty" rhsvalue=""/>
  </conditions>
  <exec from="PROCESS_ID" key="PROCESS_ID" to="EXPENSES" type="copyintolist"/>
  <exec argument="EXPENSES" type="loop">
   <exec statement="insertActionExpenses" type="insert"/>
  </exec>
 </exec>
</event>
