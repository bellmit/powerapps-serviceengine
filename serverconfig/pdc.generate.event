<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event transactional="false" type="event">
 <exec count="NO_OF_CHEQUE" to="PDC_LIST" type="createlist"/>
 <exec statement="selectNegativeOne" type="select"/>
 <exec lhs="NEGATIVE_ONE" rhs="NEGATIVE_ONE" target="ONE" type="multiply"/>
 <exec from="PDC_BANK_ID" key="PDC_BANK_ID" to="PDC_LIST" type="copyintolist"/>
 <exec from="BANK_BRANCH_ID" key="PDC_BANK_BRANCH_ID" to="PDC_LIST" type="copyintolist"/>
 <exec from="PDC_BNK_ACC_NO" key="PDC_BNK_ACC_NO" to="PDC_LIST" type="copyintolist"/>
 <exec from="PDC_CHEQUE_AMOUNT" key="PDC_CHEQUE_AMOUNT" to="PDC_LIST" type="copyintolist"/>
 <exec from="PDC_CHEQUE_DATE" key="PDC_CHEQUE_DATE" to="PDC_LIST" type="copyintolist"/>
 <exec from="PDC_CHEQUE_NUMBER" key="PDC_CHEQUE_NUMBER" to="PDC_LIST" type="copyintolist"/>
 <exec from="CHEQUE_TYPE" key="PDC_CHEQUE_TYPE" to="PDC_LIST" type="copyintolist"/>
 <exec from="PDC_LOCATION_PLACE" key="PDC_LOCATION_PLACE" to="PDC_LIST" type="copyintolist"/>
 <exec from="NO_OF_CHEQUE" key="NO_OF_CHEQUE" to="PDC_LIST" type="copyintolist"/>
 <exec from="NEGATIVE_ONE" key="NEGATIVE_ONE" to="PDC_LIST" type="copyintolist"/>
 <exec from="ZERO" key="ZERO" to="PDC_LIST" type="copyintolist"/>
 <exec from="PROCESS_REMARKS" key="PROCESS_REMARKS" to="PDC_LIST" type="copyintolist"/>
 <exec from="ONE" key="ONE" to="PDC_LIST" type="copyintolist"/>
 <exec from="PDC_CHEQUE_NUMBER" key="PDC_CHEQUE_NUMBER_BEF" to="PDC_LIST" type="copyintolist"/>
 <exec argument="PDC_LIST" index="INDEX" type="loop">
  <exec lhs="INDEX" rhs="NEGATIVE_ONE" target="NUMBER" type="add"/>
  <exec statement="getPdcChequeNumber" type="select"/>
  <exec type="guard">
   <conditions>
    <condition lhsfield="INDEX" operator="&lt;&gt;" rhsvalue="1"/>
   </conditions>
   <exec addmonths="NUMBER" ceiling="false" endmonth="false" from="PDC_CHEQUE_DATE" nextweekday="false" startmonth="false" truncate="true" type="date"/>
  </exec>
  <exec statement="formatChequeNumber" type="select"/>
 </exec>
 <exec count="1" type="createlist"/>
 <exec from="PDC_LIST" key="PDC_LIST" type="copyintolist"/>
 <exec from="TRUE" key="CLOSE" type="copyintolist"/>
</event>
